<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysUiControl'>  <resultMap type='strosoft.sys.data.SysUiControl' id='SysUiControlResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='sys_user_id' property='sysUserId' />    <result column='create_time' property='createTime' />  </resultMap>  <insert id='insertSysUiControl' parameterType='strosoft.sys.data.SysUiControl'>    insert into sys_ui_control    (code,name,description,display_order,sys_user_id,create_time)    values    (#{code},#{name},#{description},#{displayOrder},#{sysUserId},now())  </insert>  <select id='selectSysUiControl' parameterType='java.lang.String' resultMap='SysUiControlResultMap'>    select * from sys_ui_control where code=#{code}  </select>  <update id='updateSysUiControl' parameterType='strosoft.sys.data.SysUiControl'>    update sys_ui_control  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   <if test="sysUserId !=null">sys_user_id=#{sysUserId},</if>   <if test="createTime !=null">create_time=#{createTime},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysUiControl' parameterType='java.lang.String'>    delete from sys_ui_control where code=#{code}  </delete>  <select id='selectSysUiControlBySysUserId' parameterType='java.lang.Integer' resultMap='SysUiControlResultMap'>    select * from sys_ui_control where sys_user_id=#{sysUserId}  </select>  <delete id='deleteSysUiControlBySysUserId' parameterType='java.lang.Integer'>    delete from sys_ui_control where sys_user_id=#{sysUserId}  </delete>  <insert id='insertSysUiControls' parameterType='java.util.List'>    insert into sys_ui_control    (code,name,description,display_order,sys_user_id,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder},#{item.sysUserId},now())   </foreach>  </insert>  <select id='selectSysUiControls' resultMap='SysUiControlResultMap'>    select * from sys_ui_control  </select>  <select id='selectSysUiControlBySql' parameterType='java.lang.String' resultMap='SysUiControlResultMap'>    ${value}  </select>  <delete id='deleteSysUiControlBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysUiControlBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysUiControlByCondition' parameterType='java.lang.String' resultMap='SysUiControlResultMap'>    select * from sys_ui_control where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_ui_control where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>