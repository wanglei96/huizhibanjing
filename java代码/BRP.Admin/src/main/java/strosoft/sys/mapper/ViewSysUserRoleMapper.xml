<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewSysUserRole'>  <resultMap type='strosoft.sys.data.ViewSysUserRole' id='ViewSysUserRoleResultMap' >    <result column='sys_user_id' property='sysUserId' />    <result column='sys_role_id' property='sysRoleId' />    <result column='sys_user_name' property='sysUserName' />    <result column='sys_role_name' property='sysRoleName' />  </resultMap>  <select id='selectViewSysUserRoles' resultMap='ViewSysUserRoleResultMap'>    select * from view_sys_user_role  </select>  <select id='selectViewSysUserRoleBySql' parameterType='java.lang.String' resultMap='ViewSysUserRoleResultMap'>    ${value}  </select>  <select id='selectViewSysUserRoleByCondition' parameterType='java.lang.String' resultMap='ViewSysUserRoleResultMap'>    select * from view_sys_user_role where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_sys_user_role where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>