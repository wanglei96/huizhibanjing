<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewSysBasicTable'>  <resultMap type='strosoft.sys.data.ViewSysBasicTable' id='ViewSysBasicTableResultMap' >    <result column='id' property='id' />    <result column='table_name' property='tableName' />    <result column='title' property='title' />    <result column='sys_basic_table_type_code' property='sysBasicTableTypeCode' />    <result column='description' property='description' />    <result column='sys_basic_table_type_name' property='sysBasicTableTypeName' />  </resultMap>  <select id='selectViewSysBasicTables' resultMap='ViewSysBasicTableResultMap'>    select * from view_sys_basic_table  </select>  <select id='selectViewSysBasicTableById' parameterType='java.lang.Integer' resultMap='ViewSysBasicTableResultMap'>    select * from view_sys_basic_table where id= #{id}  </select>  <select id='selectViewSysBasicTableBySql' parameterType='java.lang.String' resultMap='ViewSysBasicTableResultMap'>    ${value}  </select>  <select id='selectViewSysBasicTableByCondition' parameterType='java.lang.String' resultMap='ViewSysBasicTableResultMap'>    select * from view_sys_basic_table where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_sys_basic_table where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>