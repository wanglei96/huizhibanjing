<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysRole'>  <resultMap type='strosoft.sys.data.SysRole' id='SysRoleResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='create_time' property='createTime' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertSysRole' parameterType='strosoft.sys.data.SysRole' keyProperty='id' useGeneratedKeys='true'>    insert into sys_role    (id,name,create_time,description,display_order)    values    (#{id},#{name},now(),#{description},#{displayOrder})  </insert>  <select id='selectSysRole' parameterType='java.lang.Integer' resultMap='SysRoleResultMap'>    select * from sys_role where id=#{id}  </select>  <update id='updateSysRole' parameterType='strosoft.sys.data.SysRole'>    update sys_role set id=#{id},name=#{name},create_time=#{createTime},description=#{description},display_order=#{displayOrder}    where id=#{id}  </update>  <delete id='deleteSysRole' parameterType='java.lang.Integer'>    delete from sys_role where id=#{id}  </delete>  <insert id='insertSysRoles' parameterType='java.util.List'>    insert into sys_role    (id,name,create_time,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},now(),#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectSysRoles' resultMap='SysRoleResultMap'>    select * from sys_role  </select>  <select id='selectSysRoleBySql' parameterType='java.lang.String' resultMap='SysRoleResultMap'>    ${value}  </select>  <delete id='deleteSysRoleBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysRoleBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysRoleByCondition' parameterType='java.lang.String' resultMap='SysRoleResultMap'>    select * from sys_role where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_role where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>