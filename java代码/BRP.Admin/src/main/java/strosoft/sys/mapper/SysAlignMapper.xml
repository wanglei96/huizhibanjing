<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysAlign'>  <resultMap type='strosoft.sys.data.SysAlign' id='SysAlignResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertSysAlign' parameterType='strosoft.sys.data.SysAlign'>    insert into sys_align    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectSysAlign' parameterType='java.lang.String' resultMap='SysAlignResultMap'>    select * from sys_align where code=#{code}  </select>  <update id='updateSysAlign' parameterType='strosoft.sys.data.SysAlign'>    update sys_align  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysAlign' parameterType='java.lang.String'>    delete from sys_align where code=#{code}  </delete>  <insert id='insertSysAligns' parameterType='java.util.List'>    insert into sys_align    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectSysAligns' resultMap='SysAlignResultMap'>    select * from sys_align  </select>  <select id='selectSysAlignBySql' parameterType='java.lang.String' resultMap='SysAlignResultMap'>    ${value}  </select>  <delete id='deleteSysAlignBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysAlignBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysAlignByCondition' parameterType='java.lang.String' resultMap='SysAlignResultMap'>    select * from sys_align where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_align where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>