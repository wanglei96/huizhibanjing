<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsTemplateType'>  <resultMap type='strosoft.cms.data.CmsTemplateType' id='CmsTemplateTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertCmsTemplateType' parameterType='strosoft.cms.data.CmsTemplateType'>    insert into cms_template_type    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectCmsTemplateType' parameterType='java.lang.String' resultMap='CmsTemplateTypeResultMap'>    select * from cms_template_type where code=#{code}  </select>  <update id='updateCmsTemplateType' parameterType='strosoft.cms.data.CmsTemplateType'>    update cms_template_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteCmsTemplateType' parameterType='java.lang.String'>    delete from cms_template_type where code=#{code}  </delete>  <insert id='insertCmsTemplateTypes' parameterType='java.util.List'>    insert into cms_template_type    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectCmsTemplateTypes' resultMap='CmsTemplateTypeResultMap'>    select * from cms_template_type  </select>  <select id='selectCmsTemplateTypeBySql' parameterType='java.lang.String' resultMap='CmsTemplateTypeResultMap'>    ${value}  </select>  <delete id='deleteCmsTemplateTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsTemplateTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsTemplateTypeByCondition' parameterType='java.lang.String' resultMap='CmsTemplateTypeResultMap'>    select * from cms_template_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_template_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>