<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewCmsSite'>  <resultMap type='strosoft.cms.data.ViewCmsSite' id='ViewCmsSiteResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='site_type_code' property='siteTypeCode' />    <result column='description' property='description' />    <result column='path' property='path' />    <result column='display_order' property='displayOrder' />    <result column='typeName' property='typeName' />  </resultMap>  <select id='selectViewCmsSites' resultMap='ViewCmsSiteResultMap'>    select * from view_cms_site  </select>  <select id='selectViewCmsSiteById' parameterType='java.lang.Integer' resultMap='ViewCmsSiteResultMap'>    select * from view_cms_site where id= #{id}  </select>  <select id='selectViewCmsSiteBySql' parameterType='java.lang.String' resultMap='ViewCmsSiteResultMap'>    ${value}  </select>  <select id='selectViewCmsSiteByCondition' parameterType='java.lang.String' resultMap='ViewCmsSiteResultMap'>    select * from view_cms_site where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_cms_site where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>