<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='Grade'>  <resultMap type='BRP.data.Grade' id='GradeResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />    <result column='is_deleted' property='isDeleted' />  </resultMap>  <insert id='insertGrade' parameterType='BRP.data.Grade' keyProperty='id' useGeneratedKeys='true'>    insert into grade    (id,name,description,display_order,enabled,create_time,update_time,is_deleted)    values    (#{id},#{name},#{description},#{displayOrder},#{enabled},now(),now(),#{isDeleted})  </insert>  <select id='selectGrade' parameterType='java.lang.Integer' resultMap='GradeResultMap'>    select * from grade where id=#{id}  </select>  <update id='updateGrade' parameterType='BRP.data.Grade'>    update grade set id=#{id},name=#{name},description=#{description},display_order=#{displayOrder},enabled=#{enabled},create_time=#{createTime},update_time=#{updateTime},is_deleted=#{isDeleted}    where id=#{id}  </update>  <delete id='deleteGrade' parameterType='java.lang.Integer'>    delete from grade where id=#{id}  </delete>  <insert id='insertGrades' parameterType='java.util.List'>    insert into grade    (id,name,description,display_order,enabled,create_time,update_time,is_deleted)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.description},#{item.displayOrder},#{item.enabled},now(),#{item.updateTime},#{item.isDeleted})   </foreach>  </insert>  <select id='selectGrades' resultMap='GradeResultMap'>    select * from grade  </select>  <select id='selectGradeBySql' parameterType='java.lang.String' resultMap='GradeResultMap'>    ${value}  </select>  <delete id='deleteGradeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateGradeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectGradeByCondition' parameterType='java.lang.String' resultMap='GradeResultMap'>    select * from grade where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from grade where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>