<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='DeliverArea'>  <resultMap type='BRP.data.DeliverArea' id='DeliverAreaResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='company_id' property='companyId' />    <result column='description' property='description' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertDeliverArea' parameterType='BRP.data.DeliverArea' keyProperty='id' useGeneratedKeys='true'>    insert into deliver_area    (id,name,company_id,description,create_time,update_time)    values    (#{id},#{name},#{companyId},#{description},now(),now())  </insert>  <select id='selectDeliverArea' parameterType='java.lang.Integer' resultMap='DeliverAreaResultMap'>    select * from deliver_area where id=#{id}  </select>  <update id='updateDeliverArea' parameterType='BRP.data.DeliverArea'>    update deliver_area set id=#{id},name=#{name},company_id=#{companyId},description=#{description},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteDeliverArea' parameterType='java.lang.Integer'>    delete from deliver_area where id=#{id}  </delete>  <select id='selectDeliverAreaByCompanyId' parameterType='java.lang.Integer' resultMap='DeliverAreaResultMap'>    select * from deliver_area where company_id=#{companyId}  </select>  <delete id='deleteDeliverAreaByCompanyId' parameterType='java.lang.Integer'>    delete from deliver_area where company_id=#{companyId}  </delete>  <insert id='insertDeliverAreas' parameterType='java.util.List'>    insert into deliver_area    (id,name,company_id,description,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.companyId},#{item.description},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectDeliverAreas' resultMap='DeliverAreaResultMap'>    select * from deliver_area  </select>  <select id='selectDeliverAreaBySql' parameterType='java.lang.String' resultMap='DeliverAreaResultMap'>    ${value}  </select>  <delete id='deleteDeliverAreaBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateDeliverAreaBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectDeliverAreaByCondition' parameterType='java.lang.String' resultMap='DeliverAreaResultMap'>    select * from deliver_area where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from deliver_area where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>