<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='AgeGroup'>  <resultMap type='BRP.data.AgeGroup' id='AgeGroupResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />    <result column='is_deleted' property='isDeleted' />  </resultMap>  <insert id='insertAgeGroup' parameterType='BRP.data.AgeGroup' keyProperty='id' useGeneratedKeys='true'>    insert into age_group    (id,name,description,display_order,enabled,create_time,update_time,is_deleted)    values    (#{id},#{name},#{description},#{displayOrder},#{enabled},now(),now(),#{isDeleted})  </insert>  <select id='selectAgeGroup' parameterType='java.lang.Integer' resultMap='AgeGroupResultMap'>    select * from age_group where id=#{id}  </select>  <update id='updateAgeGroup' parameterType='BRP.data.AgeGroup'>    update age_group set id=#{id},name=#{name},description=#{description},display_order=#{displayOrder},enabled=#{enabled},create_time=#{createTime},update_time=#{updateTime},is_deleted=#{isDeleted}    where id=#{id}  </update>  <delete id='deleteAgeGroup' parameterType='java.lang.Integer'>    delete from age_group where id=#{id}  </delete>  <insert id='insertAgeGroups' parameterType='java.util.List'>    insert into age_group    (id,name,description,display_order,enabled,create_time,update_time,is_deleted)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.description},#{item.displayOrder},#{item.enabled},now(),#{item.updateTime},#{item.isDeleted})   </foreach>  </insert>  <select id='selectAgeGroups' resultMap='AgeGroupResultMap'>    select * from age_group  </select>  <select id='selectAgeGroupBySql' parameterType='java.lang.String' resultMap='AgeGroupResultMap'>    ${value}  </select>  <delete id='deleteAgeGroupBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateAgeGroupBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectAgeGroupByCondition' parameterType='java.lang.String' resultMap='AgeGroupResultMap'>    select * from age_group where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from age_group where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>