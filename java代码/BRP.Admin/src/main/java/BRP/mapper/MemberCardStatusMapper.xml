e code=#{code}  </delete>  <insert id='insertMemberCardStatuss' parameterType='java.util.List'>    insert into member_card_status    (code,name,description,display_order,enabled,create_time,update_time,is_deleted)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder},#{item.enabled},now(),#{item.updateTime},#{item.isDeleted})   </foreach>  </insert>  <select id='selectMemberCardStatuss' resultMap='MemberCardStatusResultMap'>    select * from member_card_status  </select>  <select id='selectMemberCardStatusBySql' parameterType='java.lang.String' resultMap='MemberCardStatusResultMap'>    ${value}  </select>  <delete id='deleteMemberCardStatusBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateMemberCardStatusBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectMemberCardStatusByCondition' parameterType='java.lang.String' resultMap='MemberCardStatusResultMap'>    select * from member_card_status where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from member_card_status where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>