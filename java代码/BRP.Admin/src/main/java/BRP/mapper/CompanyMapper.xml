e_phone,customer_service_image_file_id,promotion_copywriting_image_file_id,bookbox_inventory_quantity,image_file_id,bank_name,account_name,bank_card_no,create_time,update_time)    values    (#{id},#{companyName},#{loginName},#{password},#{licenseImageFileId},#{companyNo},#{contact},#{mobilePhone},#{address},#{description},#{remark},#{enabled},#{deleted},#{isDefault},#{servicePhone},#{customerServiceImageFileId},#{promotionCopywritingImageFileId},#{bookboxInventoryQuantity},#{imageFileId},#{bankName},#{accountName},#{bankCardNo},now(),now())  </insert>  <select id='selectCompany' parameterType='java.lang.Integer' resultMap='CompanyResultMap'>    select * from company where id=#{id}  </select>  <update id='updateCompany' parameterType='BRP.data.Company'>    update company set id=#{id},company_name=#{companyName},login_name=#{loginName},password=#{password},license_image_file_id=#{licenseImageFileId},company_no=#{companyNo},contact=#{contact},mobile_phone=#{mobilePhone},address=#{address},description=#{description},remark=#{remark},enabled=#{enabled},deleted=#{deleted},is_default=#{isDefault},service_phone=#{servicePhone},customer_service_image_file_id=#{customerServiceImageFileId},promotion_copywriting_image_file_id=#{promotionCopywritingImageFileId},bookbox_inventory_quantity=#{bookboxInventoryQuantity},image_file_id=#{imageFileId},bank_name=#{bankName},account_name=#{accountName},bank_card_no=#{bankCardNo},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteCompany' parameterType='java.lang.Integer'>    delete from company where id=#{id}  </delete>  <select id='selectCompanyByLicenseImageFileId' parameterType='java.lang.Integer' resultMap='CompanyResultMap'>    select * from company where license_image_file_id=#{licenseImageFileId}  </select>  <delete id='deleteCompanyByLicenseImageFileId' parameterType='java.lang.Integer'>    delete from company where license_image_file_id=#{licenseImageFileId}  </d