<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanyRole'>  <resultMap type='BRP.data.CompanyRole' id='CompanyRoleResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='create_time' property='createTime' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='company_id' property='companyId' />  </resultMap>  <insert id='insertCompanyRole' parameterType='BRP.data.CompanyRole' keyProperty='id' useGeneratedKeys='true'>    insert into company_role    (id,name,create_time,description,display_order,company_id)    values    (#{id},#{name},now(),#{description},#{displayOrder},#{companyId})  </insert>  <select id='selectCompanyRole' parameterType='java.lang.Integer' resultMap='CompanyRoleResultMap'>    select * from company_role where id=#{id}  </select>  <update id='updateCompanyRole' parameterType='BRP.data.CompanyRole'>    update company_role set id=#{id},name=#{name},create_time=#{createTime},description=#{description},display_order=#{displayOrder},company_id=#{companyId}    where id=#{id}  </update>  <delete id='deleteCompanyRole' parameterType='java.lang.Integer'>    delete from company_role where id=#{id}  </delete>  <select id='selectCompanyRoleByCompanyId' parameterType='java.lang.Integer' resultMap='CompanyRoleResultMap'>    select * from company_role where company_id=#{companyId}  </select>  <delete id='deleteCompanyRoleByCompanyId' parameterType='java.lang.Integer'>    delete from company_role where company_id=#{companyId}  </delete>  <insert id='insertCompanyRoles' parameterType='java.util.List'>    insert into company_role    (id,name,create_time,description,display_order,company_id)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},now(),#{item.description},#{item.displayOrder},#{item.companyId})   </foreach>  </insert>  <select id='selectCompanyRoles' resultMap='CompanyRoleResultMap'>    select * from company_role  </select>  <select id='selectCompanyRoleBySql' parameterType='java.lang.String' resultMap='CompanyRoleResultMap'>    ${value}  </select>  <delete id='deleteCompanyRoleBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanyRoleBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanyRoleByCondition' parameterType='java.lang.String' resultMap='CompanyRoleResultMap'>    select * from company_role where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_role where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>