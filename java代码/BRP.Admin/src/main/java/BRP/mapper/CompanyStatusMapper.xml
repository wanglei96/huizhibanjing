<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanyStatus'>  <resultMap type='BRP.data.CompanyStatus' id='CompanyStatusResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertCompanyStatus' parameterType='BRP.data.CompanyStatus'>    insert into company_status    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectCompanyStatus' parameterType='java.lang.String' resultMap='CompanyStatusResultMap'>    select * from company_status where code=#{code}  </select>  <update id='updateCompanyStatus' parameterType='BRP.data.CompanyStatus'>    update company_status  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteCompanyStatus' parameterType='java.lang.String'>    delete from company_status where code=#{code}  </delete>  <insert id='insertCompanyStatuss' parameterType='java.util.List'>    insert into company_status    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectCompanyStatuss' resultMap='CompanyStatusResultMap'>    select * from company_status  </select>  <select id='selectCompanyStatusBySql' parameterType='java.lang.String' resultMap='CompanyStatusResultMap'>    ${value}  </select>  <delete id='deleteCompanyStatusBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanyStatusBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanyStatusByCondition' parameterType='java.lang.String' resultMap='CompanyStatusResultMap'>    select * from company_status where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_status where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>