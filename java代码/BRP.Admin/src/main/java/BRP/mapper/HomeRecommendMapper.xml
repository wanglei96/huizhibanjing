<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='HomeRecommend'>  <resultMap type='BRP.data.HomeRecommend' id='HomeRecommendResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='company_id' property='companyId' />    <result column='club_activity_id' property='clubActivityId' />    <result column='display_order' property='displayOrder' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertHomeRecommend' parameterType='BRP.data.HomeRecommend' keyProperty='id' useGeneratedKeys='true'>    insert into home_recommend    (id,name,company_id,club_activity_id,display_order,enabled,create_time,update_time)    values    (#{id},#{name},#{companyId},#{clubActivityId},#{displayOrder},#{enabled},now(),now())  </insert>  <select id='selectHomeRecommend' parameterType='java.lang.Integer' resultMap='HomeRecommendResultMap'>    select * from home_recommend where id=#{id}  </select>  <update id='updateHomeRecommend' parameterType='BRP.data.HomeRecommend'>    update home_recommend set id=#{id},name=#{name},company_id=#{companyId},club_activity_id=#{clubActivityId},display_order=#{displayOrder},enabled=#{enabled},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteHomeRecommend' parameterType='java.lang.Integer'>    delete from home_recommend where id=#{id}  </delete>  <select id='selectHomeRecommendByClubActivityId' parameterType='java.lang.Integer' resultMap='HomeRecommendResultMap'>    select * from home_recommend where club_activity_id=#{clubActivityId}  </select>  <select id='selectHomeRecommendByCompanyId' parameterType='java.lang.Integer' resultMap='HomeRecommendResultMap'>    select * from home_recommend where company_id=#{companyId}  </select>  <delete id='deleteHomeRecommendByClubActivityId' parameterType='java.lang.Integer'>    delete from home_recommend where club_activity_id=#{clubActivityId}  </delete>  <delete id='deleteHomeRecommendByCompanyId' parameterType='java.lang.Integer'>    delete from home_recommend where company_id=#{companyId}  </delete>  <insert id='insertHomeRecommends' parameterType='java.util.List'>    insert into home_recommend    (id,name,company_id,club_activity_id,display_order,enabled,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.companyId},#{item.clubActivityId},#{item.displayOrder},#{item.enabled},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectHomeRecommends' resultMap='HomeRecommendResultMap'>    select * from home_recommend  </select>  <select id='selectHomeRecommendBySql' parameterType='java.lang.String' resultMap='HomeRecommendResultMap'>    ${value}  </select>  <delete id='deleteHomeRecommendBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateHomeRecommendBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectHomeRecommendByCondition' parameterType='java.lang.String' resultMap='HomeRecommendResultMap'>    select * from home_recommend where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from home_recommend where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>