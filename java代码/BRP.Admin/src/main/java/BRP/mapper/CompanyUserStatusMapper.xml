<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanyUserStatus'>  <resultMap type='BRP.data.CompanyUserStatus' id='CompanyUserStatusResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertCompanyUserStatus' parameterType='BRP.data.CompanyUserStatus'>    insert into company_user_status    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectCompanyUserStatus' parameterType='java.lang.String' resultMap='CompanyUserStatusResultMap'>    select * from company_user_status where code=#{code}  </select>  <update id='updateCompanyUserStatus' parameterType='BRP.data.CompanyUserStatus'>    update company_user_status  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteCompanyUserStatus' parameterType='java.lang.String'>    delete from company_user_status where code=#{code}  </delete>  <insert id='insertCompanyUserStatuss' parameterType='java.util.List'>    insert into company_user_status    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectCompanyUserStatuss' resultMap='CompanyUserStatusResultMap'>    select * from company_user_status  </select>  <select id='selectCompanyUserStatusBySql' parameterType='java.lang.String' resultMap='CompanyUserStatusResultMap'>    ${value}  </select>  <delete id='deleteCompanyUserStatusBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanyUserStatusBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanyUserStatusByCondition' parameterType='java.lang.String' resultMap='CompanyUserStatusResultMap'>    select * from company_user_status where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_user_status where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>