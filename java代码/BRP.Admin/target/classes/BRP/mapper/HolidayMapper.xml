<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='Holiday'>  <resultMap type='BRP.data.Holiday' id='HolidayResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='vacation' property='vacation' />    <result column='remark' property='remark' />    <result column='tip' property='tip' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertHoliday' parameterType='BRP.data.Holiday' keyProperty='id' useGeneratedKeys='true'>    insert into holiday    (id,name,vacation,remark,tip,create_time,update_time)    values    (#{id},#{name},#{vacation},#{remark},#{tip},now(),now())  </insert>  <select id='selectHoliday' parameterType='java.lang.Integer' resultMap='HolidayResultMap'>    select * from holiday where id=#{id}  </select>  <update id='updateHoliday' parameterType='BRP.data.Holiday'>    update holiday set id=#{id},name=#{name},vacation=#{vacation},remark=#{remark},tip=#{tip},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteHoliday' parameterType='java.lang.Integer'>    delete from holiday where id=#{id}  </delete>  <insert id='insertHolidays' parameterType='java.util.List'>    insert into holiday    (id,name,vacation,remark,tip,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.vacation},#{item.remark},#{item.tip},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectHolidays' resultMap='HolidayResultMap'>    select * from holiday  </select>  <select id='selectHolidayBySql' parameterType='java.lang.String' resultMap='HolidayResultMap'>    ${value}  </select>  <delete id='deleteHolidayBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateHolidayBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectHolidayByCondition' parameterType='java.lang.String' resultMap='HolidayResultMap'>    select * from holiday where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from holiday where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>