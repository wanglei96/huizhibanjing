<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='HomeNavigation'>  <resultMap type='BRP.data.HomeNavigation' id='HomeNavigationResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='name' property='name' />    <result column='path' property='path' />    <result column='introduction' property='introduction' />    <result column='display_order' property='displayOrder' />    <result column='image_file_id' property='imageFileId' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertHomeNavigation' parameterType='BRP.data.HomeNavigation' keyProperty='id' useGeneratedKeys='true'>    insert into home_navigation    (id,company_id,name,path,introduction,display_order,image_file_id,enabled,create_time,update_time)    values    (#{id},#{companyId},#{name},#{path},#{introduction},#{displayOrder},#{imageFileId},#{enabled},now(),now())  </insert>  <select id='selectHomeNavigation' parameterType='java.lang.Integer' resultMap='HomeNavigationResultMap'>    select * from home_navigation where id=#{id}  </select>  <update id='updateHomeNavigation' parameterType='BRP.data.HomeNavigation'>    update home_navigation set id=#{id},company_id=#{companyId},name=#{name},path=#{path},introduction=#{introduction},display_order=#{displayOrder},image_file_id=#{imageFileId},enabled=#{enabled},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteHomeNavigation' parameterType='java.lang.Integer'>    delete from home_navigation where id=#{id}  </delete>  <select id='selectHomeNavigationByCompanyId' parameterType='java.lang.Integer' resultMap='HomeNavigationResultMap'>    select * from home_navigation where company_id=#{companyId}  </select>  <delete id='deleteHomeNavigationByCompanyId' parameterType='java.lang.Integer'>    delete from home_navigation where company_id=#{companyId}  </delete>  <insert id='insertHomeNavigations' parameterType='java.util.List'>    insert into home_navigation    (id,company_id,name,path,introduction,display_order,image_file_id,enabled,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.name},#{item.path},#{item.introduction},#{item.displayOrder},#{item.imageFileId},#{item.enabled},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectHomeNavigations' resultMap='HomeNavigationResultMap'>    select * from home_navigation  </select>  <select id='selectHomeNavigationBySql' parameterType='java.lang.String' resultMap='HomeNavigationResultMap'>    ${value}  </select>  <delete id='deleteHomeNavigationBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateHomeNavigationBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectHomeNavigationByCondition' parameterType='java.lang.String' resultMap='HomeNavigationResultMap'>    select * from home_navigation where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from home_navigation where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>