<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewMessage'>  <resultMap type='BRP.data.ViewMessage' id='ViewMessageResultMap' >    <result column='id' property='id' />    <result column='member_id' property='memberId' />    <result column='push_time' property='pushTime' />    <result column='message_type_code' property='messageTypeCode' />    <result column='content' property='content' />    <result column='company_id' property='companyId' />    <result column='msg' property='msg' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />    <result column='company_name' property='companyName' />    <result column='company_no' property='companyNo' />    <result column='member_name' property='memberName' />    <result column='message_type_name' property='messageTypeName' />  </resultMap>  <select id='selectViewMessages' resultMap='ViewMessageResultMap'>    select * from view_message  </select>  <select id='selectViewMessageById' parameterType='java.lang.Integer' resultMap='ViewMessageResultMap'>    select * from view_message where id= #{id}  </select>  <select id='selectViewMessageBySql' parameterType='java.lang.String' resultMap='ViewMessageResultMap'>    ${value}  </select>  <select id='selectViewMessageByCondition' parameterType='java.lang.String' resultMap='ViewMessageResultMap'>    select * from view_message where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_message where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>