<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='MessageBook'>  <resultMap type='BRP.data.MessageBook' id='MessageBookResultMap' >    <result column='id' property='id' />    <result column='message_id' property='messageId' />    <result column='book_id' property='bookId' />    <result column='create_time' property='createTime' />  </resultMap>  <insert id='insertMessageBook' parameterType='BRP.data.MessageBook' keyProperty='id' useGeneratedKeys='true'>    insert into message_book    (id,message_id,book_id,create_time)    values    (#{id},#{messageId},#{bookId},now())  </insert>  <select id='selectMessageBook' parameterType='java.lang.Integer' resultMap='MessageBookResultMap'>    select * from message_book where id=#{id}  </select>  <update id='updateMessageBook' parameterType='BRP.data.MessageBook'>    update message_book set id=#{id},message_id=#{messageId},book_id=#{bookId},create_time=#{createTime}    where id=#{id}  </update>  <delete id='deleteMessageBook' parameterType='java.lang.Integer'>    delete from message_book where id=#{id}  </delete>  <insert id='insertMessageBooks' parameterType='java.util.List'>    insert into message_book    (id,message_id,book_id,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.messageId},#{item.bookId},now())   </foreach>  </insert>  <select id='selectMessageBooks' resultMap='MessageBookResultMap'>    select * from message_book  </select>  <select id='selectMessageBookBySql' parameterType='java.lang.String' resultMap='MessageBookResultMap'>    ${value}  </select>  <delete id='deleteMessageBookBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateMessageBookBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectMessageBookByCondition' parameterType='java.lang.String' resultMap='MessageBookResultMap'>    select * from message_book where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from message_book where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>