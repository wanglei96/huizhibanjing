<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='RequiredBook'>  <resultMap type='BRP.data.RequiredBook' id='RequiredBookResultMap' >    <result column='id' property='id' />    <result column='book_sku_id' property='bookSkuId' />    <result column='required_book_category_id' property='requiredBookCategoryId' />    <result column='create_time' property='createTime' />  </resultMap>  <insert id='insertRequiredBook' parameterType='BRP.data.RequiredBook' keyProperty='id' useGeneratedKeys='true'>    insert into required_book    (id,book_sku_id,required_book_category_id,create_time)    values    (#{id},#{bookSkuId},#{requiredBookCategoryId},now())  </insert>  <select id='selectRequiredBook' parameterType='java.lang.Integer' resultMap='RequiredBookResultMap'>    select * from required_book where id=#{id}  </select>  <update id='updateRequiredBook' parameterType='BRP.data.RequiredBook'>    update required_book set id=#{id},book_sku_id=#{bookSkuId},required_book_category_id=#{requiredBookCategoryId},create_time=#{createTime}    where id=#{id}  </update>  <delete id='deleteRequiredBook' parameterType='java.lang.Integer'>    delete from required_book where id=#{id}  </delete>  <select id='selectRequiredBookByRequiredBookCategoryId' parameterType='java.lang.Integer' resultMap='RequiredBookResultMap'>    select * from required_book where required_book_category_id=#{requiredBookCategoryId}  </select>  <delete id='deleteRequiredBookByRequiredBookCategoryId' parameterType='java.lang.Integer'>    delete from required_book where required_book_category_id=#{requiredBookCategoryId}  </delete>  <insert id='insertRequiredBooks' parameterType='java.util.List'>    insert into required_book    (id,book_sku_id,required_book_category_id,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.bookSkuId},#{item.requiredBookCategoryId},now())   </foreach>  </insert>  <select id='selectRequiredBooks' resultMap='RequiredBookResultMap'>    select * from required_book  </select>  <select id='selectRequiredBookBySql' parameterType='java.lang.String' resultMap='RequiredBookResultMap'>    ${value}  </select>  <delete id='deleteRequiredBookBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateRequiredBookBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectRequiredBookByCondition' parameterType='java.lang.String' resultMap='RequiredBookResultMap'>    select * from required_book where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from required_book where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>