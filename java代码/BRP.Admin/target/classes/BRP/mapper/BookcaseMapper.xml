<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='Bookcase'>  <resultMap type='BRP.data.Bookcase' id='BookcaseResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='company_id' property='companyId' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertBookcase' parameterType='BRP.data.Bookcase' keyProperty='id' useGeneratedKeys='true'>    insert into bookcase    (id,name,description,company_id,create_time,update_time)    values    (#{id},#{name},#{description},#{companyId},now(),now())  </insert>  <select id='selectBookcase' parameterType='java.lang.Integer' resultMap='BookcaseResultMap'>    select * from bookcase where id=#{id}  </select>  <update id='updateBookcase' parameterType='BRP.data.Bookcase'>    update bookcase set id=#{id},name=#{name},description=#{description},company_id=#{companyId},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteBookcase' parameterType='java.lang.Integer'>    delete from bookcase where id=#{id}  </delete>  <select id='selectBookcaseByCompanyId' parameterType='java.lang.Integer' resultMap='BookcaseResultMap'>    select * from bookcase where company_id=#{companyId}  </select>  <delete id='deleteBookcaseByCompanyId' parameterType='java.lang.Integer'>    delete from bookcase where company_id=#{companyId}  </delete>  <insert id='insertBookcases' parameterType='java.util.List'>    insert into bookcase    (id,name,description,company_id,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.description},#{item.companyId},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectBookcases' resultMap='BookcaseResultMap'>    select * from bookcase  </select>  <select id='selectBookcaseBySql' parameterType='java.lang.String' resultMap='BookcaseResultMap'>    ${value}  </select>  <delete id='deleteBookcaseBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateBookcaseBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectBookcaseByCondition' parameterType='java.lang.String' resultMap='BookcaseResultMap'>    select * from bookcase where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from bookcase where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>