<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='DeliverLine'>  <resultMap type='BRP.data.DeliverLine' id='DeliverLineResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='deliver_area_id' property='deliverAreaId' />    <result column='deliverer_id' property='delivererId' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />    <result column='bookpicker_id' property='bookpickerId' />  </resultMap>  <insert id='insertDeliverLine' parameterType='BRP.data.DeliverLine' keyProperty='id' useGeneratedKeys='true'>    insert into deliver_line    (id,name,description,deliver_area_id,deliverer_id,create_time,update_time,bookpicker_id)    values    (#{id},#{name},#{description},#{deliverAreaId},#{delivererId},now(),now(),#{bookpickerId})  </insert>  <select id='selectDeliverLine' parameterType='java.lang.Integer' resultMap='DeliverLineResultMap'>    select * from deliver_line where id=#{id}  </select>  <update id='updateDeliverLine' parameterType='BRP.data.DeliverLine'>    update deliver_line set id=#{id},name=#{name},description=#{description},deliver_area_id=#{deliverAreaId},deliverer_id=#{delivererId},create_time=#{createTime},update_time=#{updateTime},bookpicker_id=#{bookpickerId}    where id=#{id}  </update>  <delete id='deleteDeliverLine' parameterType='java.lang.Integer'>    delete from deliver_line where id=#{id}  </delete>  <select id='selectDeliverLineByDelivererId' parameterType='java.lang.Integer' resultMap='DeliverLineResultMap'>    select * from deliver_line where deliverer_id=#{delivererId}  </select>  <select id='selectDeliverLineByDeliverAreaId' parameterType='java.lang.Integer' resultMap='DeliverLineResultMap'>    select * from deliver_line where deliver_area_id=#{deliverAreaId}  </select>  <delete id='deleteDeliverLineByDelivererId' parameterType='java.lang.Integer'>    delete from deliver_line where deliverer_id=#{delivererId}  </delete>  <delete id='deleteDeliverLineByDeliverAreaId' parameterType='java.lang.Integer'>    delete from deliver_line where deliver_area_id=#{deliverAreaId}  </delete>  <insert id='insertDeliverLines' parameterType='java.util.List'>    insert into deliver_line    (id,name,description,deliver_area_id,deliverer_id,create_time,update_time,bookpicker_id)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.description},#{item.deliverAreaId},#{item.delivererId},now(),#{item.updateTime},#{item.bookpickerId})   </foreach>  </insert>  <select id='selectDeliverLines' resultMap='DeliverLineResultMap'>    select * from deliver_line  </select>  <select id='selectDeliverLineBySql' parameterType='java.lang.String' resultMap='DeliverLineResultMap'>    ${value}  </select>  <delete id='deleteDeliverLineBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateDeliverLineBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectDeliverLineByCondition' parameterType='java.lang.String' resultMap='DeliverLineResultMap'>    select * from deliver_line where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from deliver_line where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>