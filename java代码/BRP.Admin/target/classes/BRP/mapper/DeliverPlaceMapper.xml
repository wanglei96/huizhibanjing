erLineId}  </delete>  <insert id='insertDeliverPlaces' parameterType='java.util.List'>    insert into deliver_place    (id,name,deliver_line_id,address,description,display_order,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.deliverLineId},#{item.address},#{item.description},#{item.displayOrder},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectDeliverPlaces' resultMap='DeliverPlaceResultMap'>    select * from deliver_place  </select>  <select id='selectDeliverPlaceBySql' parameterType='java.lang.String' resultMap='DeliverPlaceResultMap'>    ${value}  </select>  <delete id='deleteDeliverPlaceBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateDeliverPlaceBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectDeliverPlaceByCondition' parameterType='java.lang.String' resultMap='DeliverPlaceResultMap'>    select * from deliver_place where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from deliver_place where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>