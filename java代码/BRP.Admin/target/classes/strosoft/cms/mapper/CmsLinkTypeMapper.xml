<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsLinkType'>  <resultMap type='strosoft.cms.data.CmsLinkType' id='CmsLinkTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertCmsLinkType' parameterType='strosoft.cms.data.CmsLinkType'>    insert into cms_link_type    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectCmsLinkType' parameterType='java.lang.String' resultMap='CmsLinkTypeResultMap'>    select * from cms_link_type where code=#{code}  </select>  <update id='updateCmsLinkType' parameterType='strosoft.cms.data.CmsLinkType'>    update cms_link_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteCmsLinkType' parameterType='java.lang.String'>    delete from cms_link_type where code=#{code}  </delete>  <insert id='insertCmsLinkTypes' parameterType='java.util.List'>    insert into cms_link_type    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectCmsLinkTypes' resultMap='CmsLinkTypeResultMap'>    select * from cms_link_type  </select>  <select id='selectCmsLinkTypeBySql' parameterType='java.lang.String' resultMap='CmsLinkTypeResultMap'>    ${value}  </select>  <delete id='deleteCmsLinkTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsLinkTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsLinkTypeByCondition' parameterType='java.lang.String' resultMap='CmsLinkTypeResultMap'>    select * from cms_link_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_link_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>