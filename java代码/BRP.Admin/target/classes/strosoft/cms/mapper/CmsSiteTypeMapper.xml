<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsSiteType'>  <resultMap type='strosoft.cms.data.CmsSiteType' id='CmsSiteTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />  </resultMap>  <insert id='insertCmsSiteType' parameterType='strosoft.cms.data.CmsSiteType'>    insert into cms_site_type    (code,name,description)    values    (#{code},#{name},#{description})  </insert>  <select id='selectCmsSiteType' parameterType='java.lang.String' resultMap='CmsSiteTypeResultMap'>    select * from cms_site_type where code=#{code}  </select>  <update id='updateCmsSiteType' parameterType='strosoft.cms.data.CmsSiteType'>    update cms_site_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteCmsSiteType' parameterType='java.lang.String'>    delete from cms_site_type where code=#{code}  </delete>  <insert id='insertCmsSiteTypes' parameterType='java.util.List'>    insert into cms_site_type    (code,name,description)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description})   </foreach>  </insert>  <select id='selectCmsSiteTypes' resultMap='CmsSiteTypeResultMap'>    select * from cms_site_type  </select>  <select id='selectCmsSiteTypeBySql' parameterType='java.lang.String' resultMap='CmsSiteTypeResultMap'>    ${value}  </select>  <delete id='deleteCmsSiteTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsSiteTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsSiteTypeByCondition' parameterType='java.lang.String' resultMap='CmsSiteTypeResultMap'>    select * from cms_site_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_site_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>