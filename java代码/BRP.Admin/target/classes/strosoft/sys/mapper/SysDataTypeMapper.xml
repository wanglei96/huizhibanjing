<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysDataType'>  <resultMap type='strosoft.sys.data.SysDataType' id='SysDataTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='sys_user_id' property='sysUserId' />    <result column='create_time' property='createTime' />  </resultMap>  <insert id='insertSysDataType' parameterType='strosoft.sys.data.SysDataType'>    insert into sys_data_type    (code,name,description,display_order,sys_user_id,create_time)    values    (#{code},#{name},#{description},#{displayOrder},#{sysUserId},now())  </insert>  <select id='selectSysDataType' parameterType='java.lang.String' resultMap='SysDataTypeResultMap'>    select * from sys_data_type where code=#{code}  </select>  <update id='updateSysDataType' parameterType='strosoft.sys.data.SysDataType'>    update sys_data_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   <if test="sysUserId !=null">sys_user_id=#{sysUserId},</if>   <if test="createTime !=null">create_time=#{createTime},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysDataType' parameterType='java.lang.String'>    delete from sys_data_type where code=#{code}  </delete>  <select id='selectSysDataTypeBySysUserId' parameterType='java.lang.Integer' resultMap='SysDataTypeResultMap'>    select * from sys_data_type where sys_user_id=#{sysUserId}  </select>  <delete id='deleteSysDataTypeBySysUserId' parameterType='java.lang.Integer'>    delete from sys_data_type where sys_user_id=#{sysUserId}  </delete>  <insert id='insertSysDataTypes' parameterType='java.util.List'>    insert into sys_data_type    (code,name,description,display_order,sys_user_id,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder},#{item.sysUserId},now())   </foreach>  </insert>  <select id='selectSysDataTypes' resultMap='SysDataTypeResultMap'>    select * from sys_data_type  </select>  <select id='selectSysDataTypeBySql' parameterType='java.lang.String' resultMap='SysDataTypeResultMap'>    ${value}  </select>  <delete id='deleteSysDataTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysDataTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysDataTypeByCondition' parameterType='java.lang.String' resultMap='SysDataTypeResultMap'>    select * from sys_data_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_data_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>