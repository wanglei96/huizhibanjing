tem='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder},#{item.userId},now())   </foreach>  </insert>  <select id='selectSysBasicTableTypes' resultMap='SysBasicTableTypeResultMap'>    select * from sys_basic_table_type  </select>  <select id='selectSysBasicTableTypeBySql' parameterType='java.lang.String' resultMap='SysBasicTableTypeResultMap'>    ${value}  </select>  <delete id='deleteSysBasicTableTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysBasicTableTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysBasicTableTypeByCondition' parameterType='java.lang.String' resultMap='SysBasicTableTypeResultMap'>    select * from sys_basic_table_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_basic_table_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>