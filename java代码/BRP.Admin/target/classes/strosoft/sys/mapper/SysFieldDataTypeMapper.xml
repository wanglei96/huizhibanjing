ataTypeBySql' parameterType='java.lang.String' resultMap='SysFieldDataTypeResultMap'>    ${value}  </select>  <delete id='deleteSysFieldDataTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysFieldDataTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysFieldDataTypeByCondition' parameterType='java.lang.String' resultMap='SysFieldDataTypeResultMap'>    select * from sys_field_data_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_field_data_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>