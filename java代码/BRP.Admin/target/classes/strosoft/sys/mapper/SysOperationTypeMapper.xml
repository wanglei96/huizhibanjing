<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysOperationType'>  <resultMap type='strosoft.sys.data.SysOperationType' id='SysOperationTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertSysOperationType' parameterType='strosoft.sys.data.SysOperationType'>    insert into sys_operation_type    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectSysOperationType' parameterType='java.lang.String' resultMap='SysOperationTypeResultMap'>    select * from sys_operation_type where code=#{code}  </select>  <update id='updateSysOperationType' parameterType='strosoft.sys.data.SysOperationType'>    update sys_operation_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysOperationType' parameterType='java.lang.String'>    delete from sys_operation_type where code=#{code}  </delete>  <insert id='insertSysOperationTypes' parameterType='java.util.List'>    insert into sys_operation_type    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectSysOperationTypes' resultMap='SysOperationTypeResultMap'>    select * from sys_operation_type  </select>  <select id='selectSysOperationTypeBySql' parameterType='java.lang.String' resultMap='SysOperationTypeResultMap'>    ${value}  </select>  <delete id='deleteSysOperationTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysOperationTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysOperationTypeByCondition' parameterType='java.lang.String' resultMap='SysOperationTypeResultMap'>    select * from sys_operation_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_operation_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>