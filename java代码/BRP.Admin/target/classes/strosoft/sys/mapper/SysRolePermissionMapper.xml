<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysRolePermission'>  <resultMap type='strosoft.sys.data.SysRolePermission' id='SysRolePermissionResultMap' >    <result column='sys_role_id' property='sysRoleId' />    <result column='permission' property='permission' />  </resultMap>  <insert id='insertSysRolePermission' parameterType='strosoft.sys.data.SysRolePermission'>    insert into sys_role_permission    (sys_role_id,permission)    values    (#{sysRoleId},#{permission})  </insert>  <select id='selectSysRolePermission' parameterType='strosoft.sys.data.SysRolePermission' resultMap='SysRolePermissionResultMap'>    select * from sys_role_permission where sys_role_id=#{sysRoleId} and permission=#{permission}  </select>  <update id='updateSysRolePermission' parameterType='strosoft.sys.data.SysRolePermission'>    update sys_role_permission  <trim prefix="set" suffixOverrides=",">    <if test="sysRoleId !=null">sys_role_id=#{sysRoleId},</if>   <if test="permission !=null">permission=#{permission},</if>   </trim>   <where>    sys_role_id=#{sysRoleId} and permission=#{permission}  </where>  </update>  <delete id='deleteSysRolePermission' parameterType='strosoft.sys.data.SysRolePermission'>    delete from sys_role_permission where sys_role_id=#{sysRoleId} and permission=#{permission}  </delete>  <select id='selectSysRolePermissionBySysRoleId' parameterType='java.lang.Integer' resultMap='SysRolePermissionResultMap'>    select * from sys_role_permission where sys_role_id=#{sysRoleId}  </select>  <insert id='insertSysRolePermissions' parameterType='java.util.List'>    insert into sys_role_permission    (sys_role_id,permission)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.sysRoleId},#{item.permission})   </foreach>  </insert>  <select id='selectSysRolePermissions' resultMap='SysRolePermissionResultMap'>    select * from sys_role_permission  </select>  <select id='selectSysRolePermissionBySql' parameterType='java.lang.String' resultMap='SysRolePermissionResultMap'>    ${value}  </select>  <delete id='deleteSysRolePermissionBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysRolePermissionBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysRolePermissionByCondition' parameterType='java.lang.String' resultMap='SysRolePermissionResultMap'>    select * from sys_role_permission where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_role_permission where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>