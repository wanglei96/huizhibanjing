<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysOperationTarget'>  <resultMap type='strosoft.sys.data.SysOperationTarget' id='SysOperationTargetResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertSysOperationTarget' parameterType='strosoft.sys.data.SysOperationTarget'>    insert into sys_operation_target    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectSysOperationTarget' parameterType='java.lang.String' resultMap='SysOperationTargetResultMap'>    select * from sys_operation_target where code=#{code}  </select>  <update id='updateSysOperationTarget' parameterType='strosoft.sys.data.SysOperationTarget'>    update sys_operation_target  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysOperationTarget' parameterType='java.lang.String'>    delete from sys_operation_target where code=#{code}  </delete>  <insert id='insertSysOperationTargets' parameterType='java.util.List'>    insert into sys_operation_target    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectSysOperationTargets' resultMap='SysOperationTargetResultMap'>    select * from sys_operation_target  </select>  <select id='selectSysOperationTargetBySql' parameterType='java.lang.String' resultMap='SysOperationTargetResultMap'>    ${value}  </select>  <delete id='deleteSysOperationTargetBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysOperationTargetBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysOperationTargetByCondition' parameterType='java.lang.String' resultMap='SysOperationTargetResultMap'>    select * from sys_operation_target where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_operation_target where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>