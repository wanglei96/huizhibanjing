                    DataManagerType="strosoft.sys.model.SysUserManager"
                    DataEntityType="strosoft.sys.data.SysUser" EncryptFields="password"
                    TableName="sys_user" PrimaryKeys="id">
        <RequredFields>
            <RequiredField DataName="loginName" Caption="账号"></RequiredField>
            <RequiredField DataName="trueName" Caption="姓名"></RequiredField>
            <RequiredField DataName="password" Caption="密码"></RequiredField>
        </RequredFields>
        <UniqueFields>
            <UniqueField DataName="loginName" Caption="账号"
                         FieldName="login_name"></UniqueField>
            <UniqueField DataName="employeeNo" Caption="员工编号"
                         FieldName="employee_no"></UniqueField>
        </UniqueFields>
    </ServiceHandler>
