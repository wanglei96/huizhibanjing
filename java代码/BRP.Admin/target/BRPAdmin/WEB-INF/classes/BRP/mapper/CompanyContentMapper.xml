<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanyContent'>  <resultMap type='BRP.data.CompanyContent' id='CompanyContentResultMap' >    <result column='id' property='id' />    <result column='title' property='title' />    <result column='url' property='url' />    <result column='text' property='text' />    <result column='company_channel_id' property='companyChannelId' />    <result column='display_order' property='displayOrder' />    <result column='image_file_id' property='imageFileId' />    <result column='attachment_file_id' property='attachmentFileId' />    <result column='company_user_id' property='companyUserId' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertCompanyContent' parameterType='BRP.data.CompanyContent' keyProperty='id' useGeneratedKeys='true'>    insert into company_content    (id,title,url,text,company_channel_id,display_order,image_file_id,attachment_file_id,company_user_id,create_time,update_time)    values    (#{id},#{title},#{url},#{text},#{companyChannelId},#{displayOrder},#{imageFileId},#{attachmentFileId},#{companyUserId},now(),now())  </insert>  <select id='selectCompanyContent' parameterType='java.lang.Integer' resultMap='CompanyContentResultMap'>    select * from company_content where id=#{id}  </select>  <update id='updateCompanyContent' parameterType='BRP.data.CompanyContent'>    update company_content set id=#{id},title=#{title},url=#{url},text=#{text},company_channel_id=#{companyChannelId},display_order=#{displayOrder},image_file_id=#{imageFileId},attachment_file_id=#{attachmentFileId},company_user_id=#{companyUserId},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteCompanyContent' parameterType='java.lang.Integer'>    delete from company_content where id=#{id}  </delete>  <select id='selectCompanyContentByAttachmentFileId' parameterType='java.lang.Integer' resultMap='CompanyContentResultMap'>    select * from company_content where attachment_file_id=#{attachmentFileId}  </select>  <select id='selectCompanyContentByImageFileId' parameterType='java.lang.Integer' resultMap='CompanyContentResultMap'>    select * from company_content where image_file_id=#{imageFileId}  </select>  <delete id='deleteCompanyContentByAttachmentFileId' parameterType='java.lang.Integer'>    delete from company_content where attachment_file_id=#{attachmentFileId}  </delete>  <delete id='deleteCompanyContentByImageFileId' parameterType='java.lang.Integer'>    delete from company_content where image_file_id=#{imageFileId}  </delete>  <insert id='insertCompanyContents' parameterType='java.util.List'>    insert into company_content    (id,title,url,text,company_channel_id,display_order,image_file_id,attachment_file_id,company_user_id,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.title},#{item.url},#{item.text},#{item.companyChannelId},#{item.displayOrder},#{item.imageFileId},#{item.attachmentFileId},#{item.companyUserId},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectCompanyContents' resultMap='CompanyContentResultMap'>    select * from company_content  </select>  <select id='selectCompanyContentBySql' parameterType='java.lang.String' resultMap='CompanyContentResultMap'>    ${value}  </select>  <delete id='deleteCompanyContentBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanyContentBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanyContentByCondition' parameterType='java.lang.String' resultMap='CompanyContentResultMap'>    select * from company_content where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_content where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>