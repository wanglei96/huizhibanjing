<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewHomeNavigation'>  <resultMap type='BRP.data.ViewHomeNavigation' id='ViewHomeNavigationResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='name' property='name' />    <result column='path' property='path' />    <result column='introduction' property='introduction' />    <result column='display_order' property='displayOrder' />    <result column='image_file_id' property='imageFileId' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />    <result column='image_file_path' property='imageFilePath' />  </resultMap>  <select id='selectViewHomeNavigations' resultMap='ViewHomeNavigationResultMap'>    select * from view_home_navigation  </select>  <select id='selectViewHomeNavigationById' parameterType='java.lang.Integer' resultMap='ViewHomeNavigationResultMap'>    select * from view_home_navigation where id= #{id}  </select>  <select id='selectViewHomeNavigationBySql' parameterType='java.lang.String' resultMap='ViewHomeNavigationResultMap'>    ${value}  </select>  <select id='selectViewHomeNavigationByCondition' parameterType='java.lang.String' resultMap='ViewHomeNavigationResultMap'>    select * from view_home_navigation where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_home_navigation where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>