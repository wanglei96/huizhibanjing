<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ReadActivity'>  <resultMap type='BRP.data.ReadActivity' id='ReadActivityResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='image_file_id' property='imageFileId' />    <result column='price' property='price' />    <result column='content' property='content' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertReadActivity' parameterType='BRP.data.ReadActivity' keyProperty='id' useGeneratedKeys='true'>    insert into read_activity    (id,name,image_file_id,price,content,create_time,update_time)    values    (#{id},#{name},#{imageFileId},#{price},#{content},now(),now())  </insert>  <select id='selectReadActivity' parameterType='java.lang.Integer' resultMap='ReadActivityResultMap'>    select * from read_activity where id=#{id}  </select>  <update id='updateReadActivity' parameterType='BRP.data.ReadActivity'>    update read_activity set id=#{id},name=#{name},image_file_id=#{imageFileId},price=#{price},content=#{content},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteReadActivity' parameterType='java.lang.Integer'>    delete from read_activity where id=#{id}  </delete>  <select id='selectReadActivityByImageFileId' parameterType='java.lang.Integer' resultMap='ReadActivityResultMap'>    select * from read_activity where image_file_id=#{imageFileId}  </select>  <delete id='deleteReadActivityByImageFileId' parameterType='java.lang.Integer'>    delete from read_activity where image_file_id=#{imageFileId}  </delete>  <insert id='insertReadActivitys' parameterType='java.util.List'>    insert into read_activity    (id,name,image_file_id,price,content,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.imageFileId},#{item.price},#{item.content},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectReadActivitys' resultMap='ReadActivityResultMap'>    select * from read_activity  </select>  <select id='selectReadActivityBySql' parameterType='java.lang.String' resultMap='ReadActivityResultMap'>    ${value}  </select>  <delete id='deleteReadActivityBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateReadActivityBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectReadActivityByCondition' parameterType='java.lang.String' resultMap='ReadActivityResultMap'>    select * from read_activity where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from read_activity where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>