<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanySetting'>  <resultMap type='BRP.data.CompanySetting' id='CompanySettingResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='sys_setting_code' property='sysSettingCode' />    <result column='value' property='value' />  </resultMap>  <insert id='insertCompanySetting' parameterType='BRP.data.CompanySetting' keyProperty='id' useGeneratedKeys='true'>    insert into company_setting    (id,company_id,sys_setting_code,value)    values    (#{id},#{companyId},#{sysSettingCode},#{value})  </insert>  <select id='selectCompanySetting' parameterType='java.lang.Integer' resultMap='CompanySettingResultMap'>    select * from company_setting where id=#{id}  </select>  <update id='updateCompanySetting' parameterType='BRP.data.CompanySetting'>    update company_setting set id=#{id},company_id=#{companyId},sys_setting_code=#{sysSettingCode},value=#{value}    where id=#{id}  </update>  <delete id='deleteCompanySetting' parameterType='java.lang.Integer'>    delete from company_setting where id=#{id}  </delete>  <select id='selectCompanySettingByCompanyId' parameterType='java.lang.Integer' resultMap='CompanySettingResultMap'>    select * from company_setting where company_id=#{companyId}  </select>  <select id='selectCompanySettingBySysSettingCode' parameterType='java.lang.String' resultMap='CompanySettingResultMap'>    select * from company_setting where sys_setting_code=#{sysSettingCode}  </select>  <delete id='deleteCompanySettingByCompanyId' parameterType='java.lang.Integer'>    delete from company_setting where company_id=#{companyId}  </delete>  <delete id='deleteCompanySettingBySysSettingCode' parameterType='java.lang.String'>    delete from company_setting where sys_setting_code=#{sysSettingCode}  </delete>  <insert id='insertCompanySettings' parameterType='java.util.List'>    insert into company_setting    (id,company_id,sys_setting_code,value)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.sysSettingCode},#{item.value})   </foreach>  </insert>  <select id='selectCompanySettings' resultMap='CompanySettingResultMap'>    select * from company_setting  </select>  <select id='selectCompanySettingBySql' parameterType='java.lang.String' resultMap='CompanySettingResultMap'>    ${value}  </select>  <delete id='deleteCompanySettingBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanySettingBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanySettingByCondition' parameterType='java.lang.String' resultMap='CompanySettingResultMap'>    select * from company_setting where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_setting where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>