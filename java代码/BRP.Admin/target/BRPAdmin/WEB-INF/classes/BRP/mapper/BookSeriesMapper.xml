h>  </insert>  <select id='selectBookSeriess' resultMap='BookSeriesResultMap'>    select * from book_series  </select>  <select id='selectBookSeriesBySql' parameterType='java.lang.String' resultMap='BookSeriesResultMap'>    ${value}  </select>  <delete id='deleteBookSeriesBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateBookSeriesBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectBookSeriesByCondition' parameterType='java.lang.String' resultMap='BookSeriesResultMap'>    select * from book_series where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from book_series where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>