<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='Product'>  <resultMap type='BRP.data.Product' id='ProductResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='code' property='code' />    <result column='price' property='price' />    <result column='description' property='description' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertProduct' parameterType='BRP.data.Product' keyProperty='id' useGeneratedKeys='true'>    insert into product    (id,name,code,price,description,create_time,update_time)    values    (#{id},#{name},#{code},#{price},#{description},now(),now())  </insert>  <select id='selectProduct' parameterType='java.lang.Integer' resultMap='ProductResultMap'>    select * from product where id=#{id}  </select>  <update id='updateProduct' parameterType='BRP.data.Product'>    update product set id=#{id},name=#{name},code=#{code},price=#{price},description=#{description},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteProduct' parameterType='java.lang.Integer'>    delete from product where id=#{id}  </delete>  <insert id='insertProducts' parameterType='java.util.List'>    insert into product    (id,name,code,price,description,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.code},#{item.price},#{item.description},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectProducts' resultMap='ProductResultMap'>    select * from product  </select>  <select id='selectProductBySql' parameterType='java.lang.String' resultMap='ProductResultMap'>    ${value}  </select>  <delete id='deleteProductBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateProductBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectProductByCondition' parameterType='java.lang.String' resultMap='ProductResultMap'>    select * from product where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from product where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>