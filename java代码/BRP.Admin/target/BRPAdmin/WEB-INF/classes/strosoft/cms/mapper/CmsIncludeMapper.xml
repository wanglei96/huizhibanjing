<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsInclude'>  <resultMap type='strosoft.cms.data.CmsInclude' id='CmsIncludeResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='path' property='path' />    <result column='display_order' property='displayOrder' />    <result column='encode' property='encode' />    <result column='cms_include_content' property='cmsIncludeContent' />    <result column='cms_site_id' property='cmsSiteId' />  </resultMap>  <insert id='insertCmsInclude' parameterType='strosoft.cms.data.CmsInclude' keyProperty='id' useGeneratedKeys='true'>    insert into cms_include    (id,name,description,path,display_order,encode,cms_include_content,cms_site_id)    values    (#{id},#{name},#{description},#{path},#{displayOrder},#{encode},#{cmsIncludeContent},#{cmsSiteId})  </insert>  <select id='selectCmsInclude' parameterType='java.lang.Integer' resultMap='CmsIncludeResultMap'>    select * from cms_include where id=#{id}  </select>  <update id='updateCmsInclude' parameterType='strosoft.cms.data.CmsInclude'>    update cms_include set id=#{id},name=#{name},description=#{description},path=#{path},display_order=#{displayOrder},encode=#{encode},cms_include_content=#{cmsIncludeContent},cms_site_id=#{cmsSiteId}    where id=#{id}  </update>  <delete id='deleteCmsInclude' parameterType='java.lang.Integer'>    delete from cms_include where id=#{id}  </delete>  <insert id='insertCmsIncludes' parameterType='java.util.List'>    insert into cms_include    (id,name,description,path,display_order,encode,cms_include_content,cms_site_id)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.description},#{item.path},#{item.displayOrder},#{item.encode},#{item.cmsIncludeContent},#{item.cmsSiteId})   </foreach>  </insert>  <select id='selectCmsIncludes' resultMap='CmsIncludeResultMap'>    select * from cms_include  </select>  <select id='selectCmsIncludeBySql' parameterType='java.lang.String' resultMap='CmsIncludeResultMap'>    ${value}  </select>  <delete id='deleteCmsIncludeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsIncludeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsIncludeByCondition' parameterType='java.lang.String' resultMap='CmsIncludeResultMap'>    select * from cms_include where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_include where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>