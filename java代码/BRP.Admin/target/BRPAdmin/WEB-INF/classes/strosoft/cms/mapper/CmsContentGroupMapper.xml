<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsContentGroup'>  <resultMap type='strosoft.cms.data.CmsContentGroup' id='CmsContentGroupResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='cms_site_id' property='cmsSiteId' />  </resultMap>  <insert id='insertCmsContentGroup' parameterType='strosoft.cms.data.CmsContentGroup'>    insert into cms_content_group    (code,name,description,display_order,cms_site_id)    values    (#{code},#{name},#{description},#{displayOrder},#{cmsSiteId})  </insert>  <select id='selectCmsContentGroup' parameterType='strosoft.cms.data.CmsContentGroup' resultMap='CmsContentGroupResultMap'>    select * from cms_content_group where code=#{code} and cms_site_id=#{cmsSiteId}  </select>  <update id='updateCmsContentGroup' parameterType='strosoft.cms.data.CmsContentGroup'>    update cms_content_group  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   <if test="cmsSiteId !=null">cms_site_id=#{cmsSiteId},</if>   </trim>   <where>    code=#{code} and cms_site_id=#{cmsSiteId}  </where>  </update>  <delete id='deleteCmsContentGroup' parameterType='strosoft.cms.data.CmsContentGroup'>    delete from cms_content_group where code=#{code} and cms_site_id=#{cmsSiteId}  </delete>  <insert id='insertCmsContentGroups' parameterType='java.util.List'>    insert into cms_content_group    (code,name,description,display_order,cms_site_id)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder},#{item.cmsSiteId})   </foreach>  </insert>  <select id='selectCmsContentGroups' resultMap='CmsContentGroupResultMap'>    select * from cms_content_group  </select>  <select id='selectCmsContentGroupBySql' parameterType='java.lang.String' resultMap='CmsContentGroupResultMap'>    ${value}  </select>  <delete id='deleteCmsContentGroupBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsContentGroupBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsContentGroupByCondition' parameterType='java.lang.String' resultMap='CmsContentGroupResultMap'>    select * from cms_content_group where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_content_group where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>