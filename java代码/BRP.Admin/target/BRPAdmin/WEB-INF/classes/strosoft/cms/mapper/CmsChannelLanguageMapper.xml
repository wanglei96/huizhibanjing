java.lang.Integer'>    delete from cms_channel_language where id=#{id}  </delete>  <select id='selectCmsChannelLanguageByCmsChannelId' parameterType='java.lang.Integer' resultMap='CmsChannelLanguageResultMap'>    select * from cms_channel_language where cms_channel_id=#{cmsChannelId}  </select>  <select id='selectCmsChannelLanguageByCmsLanguageCode' parameterType='java.lang.String' resultMap='CmsChannelLanguageResultMap'>    select * from cms_channel_language where cms_language_code=#{cmsLanguageCode}  </select>  <select id='selectCmsChannelLanguageByImageFileId' parameterType='java.lang.Integer' resultMap='CmsChannelLanguageResultMap'>    select * from cms_channel_language where image_file_id=#{imageFileId}  </select>  <delete id='deleteCmsChannelLanguageByCmsChannelId' parameterType='java.lang.Integer'>    delete from cms_channel_language where cms_channel_id=#{cmsChannelId}  </delete>  <delete id='deleteCmsChannelLanguageByCmsLanguageCode' parameterType='java.lang.String'>    delete from cms_channel_language where cms_language_code=#{cmsLanguageCode}  </delete>  <delete id='deleteCmsChannelLanguageByImageFileId' parameterType='java.lang.Integer'>    delete from cms_channel_language where image_file_id=#{imageFileId}  </delete>  <insert id='insertCmsChannelLanguages' parameterType='java.util.List'>    insert into cms_channel_language    (id,name,title,keyword,url,text,description,image_file_id,cms_language_code,cms_channel_id,channel_file_path_rule)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.title},#{item.keyword},#{item.url},#{item.text},#{item.description},#{item.imageFileId},#{item.cmsLanguageCode},#{item.cmsChannelId},#{item.channelFilePathRule})   </foreach>  </insert>  <select id='selectCmsChannelLanguages' resultMap='CmsChannelLanguageResultMap'>    select * from cms_channel_language  </select>  <select id='selectCmsChannelLanguageBySql' parameterType='java.lang.String' resultMap='CmsChannelLanguageResultMap'>    ${value}  </select>  <delete id='deleteCmsChannelLanguageBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsChannelLanguageBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsChannelLanguageByCondition' parameterType='java.lang.String' resultMap='CmsChannelLanguageResultMap'>    select * from cms_channel_language where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_channel_language where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>