<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsSite'>  <resultMap type='strosoft.cms.data.CmsSite' id='CmsSiteResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='site_type_code' property='siteTypeCode' />    <result column='description' property='description' />    <result column='path' property='path' />    <result column='display_order' property='displayOrder' />    <result column='parent_id' property='parentId' />    <result column='is_cms_language' property='isCmsLanguage' />  </resultMap>  <insert id='insertCmsSite' parameterType='strosoft.cms.data.CmsSite' keyProperty='id' useGeneratedKeys='true'>    insert into cms_site    (id,name,site_type_code,description,path,display_order,parent_id,is_cms_language)    values    (#{id},#{name},#{siteTypeCode},#{description},#{path},#{displayOrder},#{parentId},#{isCmsLanguage})  </insert>  <select id='selectCmsSite' parameterType='java.lang.Integer' resultMap='CmsSiteResultMap'>    select * from cms_site where id=#{id}  </select>  <update id='updateCmsSite' parameterType='strosoft.cms.data.CmsSite'>    update cms_site set id=#{id},name=#{name},site_type_code=#{siteTypeCode},description=#{description},path=#{path},display_order=#{displayOrder},parent_id=#{parentId},is_cms_language=#{isCmsLanguage}    where id=#{id}  </update>  <delete id='deleteCmsSite' parameterType='java.lang.Integer'>    delete from cms_site where id=#{id}  </delete>  <insert id='insertCmsSites' parameterType='java.util.List'>    insert into cms_site    (id,name,site_type_code,description,path,display_order,parent_id,is_cms_language)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.siteTypeCode},#{item.description},#{item.path},#{item.displayOrder},#{item.parentId},#{item.isCmsLanguage})   </foreach>  </insert>  <select id='selectCmsSites' resultMap='CmsSiteResultMap'>    select * from cms_site  </select>  <select id='selectCmsSiteBySql' parameterType='java.lang.String' resultMap='CmsSiteResultMap'>    ${value}  </select>  <delete id='deleteCmsSiteBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsSiteBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsSiteByCondition' parameterType='java.lang.String' resultMap='CmsSiteResultMap'>    select * from cms_site where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_site where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>