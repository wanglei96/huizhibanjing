<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsContentStatus'>  <resultMap type='strosoft.cms.data.CmsContentStatus' id='CmsContentStatusResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertCmsContentStatus' parameterType='strosoft.cms.data.CmsContentStatus'>    insert into cms_content_status    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectCmsContentStatus' parameterType='java.lang.String' resultMap='CmsContentStatusResultMap'>    select * from cms_content_status where code=#{code}  </select>  <update id='updateCmsContentStatus' parameterType='strosoft.cms.data.CmsContentStatus'>    update cms_content_status  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteCmsContentStatus' parameterType='java.lang.String'>    delete from cms_content_status where code=#{code}  </delete>  <insert id='insertCmsContentStatuss' parameterType='java.util.List'>    insert into cms_content_status    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectCmsContentStatuss' resultMap='CmsContentStatusResultMap'>    select * from cms_content_status  </select>  <select id='selectCmsContentStatusBySql' parameterType='java.lang.String' resultMap='CmsContentStatusResultMap'>    ${value}  </select>  <delete id='deleteCmsContentStatusBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsContentStatusBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsContentStatusByCondition' parameterType='java.lang.String' resultMap='CmsContentStatusResultMap'>    select * from cms_content_status where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_content_status where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>