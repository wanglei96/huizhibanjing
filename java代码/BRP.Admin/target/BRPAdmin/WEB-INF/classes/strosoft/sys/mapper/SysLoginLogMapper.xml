<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysLoginLog'>  <resultMap type='strosoft.sys.data.SysLoginLog' id='SysLoginLogResultMap' >    <result column='id' property='id' />    <result column='account_id' property='accountId' />    <result column='sys_login_type_code' property='sysLoginTypeCode' />    <result column='ip' property='ip' />    <result column='error_message' property='errorMessage' />    <result column='create_time' property='createTime' />  </resultMap>  <insert id='insertSysLoginLog' parameterType='strosoft.sys.data.SysLoginLog' keyProperty='id' useGeneratedKeys='true'>    insert into sys_login_log    (id,account_id,sys_login_type_code,ip,error_message,create_time)    values    (#{id},#{accountId},#{sysLoginTypeCode},#{ip},#{errorMessage},now())  </insert>  <select id='selectSysLoginLog' parameterType='java.lang.Integer' resultMap='SysLoginLogResultMap'>    select * from sys_login_log where id=#{id}  </select>  <update id='updateSysLoginLog' parameterType='strosoft.sys.data.SysLoginLog'>    update sys_login_log set id=#{id},account_id=#{accountId},sys_login_type_code=#{sysLoginTypeCode},ip=#{ip},error_message=#{errorMessage},create_time=#{createTime}    where id=#{id}  </update>  <delete id='deleteSysLoginLog' parameterType='java.lang.Integer'>    delete from sys_login_log where id=#{id}  </delete>  <select id='selectSysLoginLogBySysLoginTypeCode' parameterType='java.lang.String' resultMap='SysLoginLogResultMap'>    select * from sys_login_log where sys_login_type_code=#{sysLoginTypeCode}  </select>  <delete id='deleteSysLoginLogBySysLoginTypeCode' parameterType='java.lang.String'>    delete from sys_login_log where sys_login_type_code=#{sysLoginTypeCode}  </delete>  <insert id='insertSysLoginLogs' parameterType='java.util.List'>    insert into sys_login_log    (id,account_id,sys_login_type_code,ip,error_message,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.accountId},#{item.sysLoginTypeCode},#{item.ip},#{item.errorMessage},now())   </foreach>  </insert>  <select id='selectSysLoginLogs' resultMap='SysLoginLogResultMap'>    select * from sys_login_log  </select>  <select id='selectSysLoginLogBySql' parameterType='java.lang.String' resultMap='SysLoginLogResultMap'>    ${value}  </select>  <delete id='deleteSysLoginLogBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysLoginLogBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysLoginLogByCondition' parameterType='java.lang.String' resultMap='SysLoginLogResultMap'>    select * from sys_login_log where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_login_log where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>