}  </update>  <delete id='deleteSysUser' parameterType='java.lang.Integer'>    delete from sys_user where id=#{id}  </delete>  <select id='selectSysUserBySysOrganizationId' parameterType='java.lang.Integer' resultMap='SysUserResultMap'>    select * from sys_user where sys_organization_id=#{sysOrganizationId}  </select>  <delete id='deleteSysUserBySysOrganizationId' parameterType='java.lang.Integer'>    delete from sys_user where sys_organization_id=#{sysOrganizationId}  </delete>  <insert id='insertSysUsers' parameterType='java.util.List'>    insert into sys_user    (id,employee_no,login_name,password,true_name,sys_organization_id,create_time,description,enabled,mobile_phone,wrong_password_count,is_deleted)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.employeeNo},#{item.loginName},#{item.password},#{item.trueName},#{item.sysOrganizationId},now(),#{item.description},#{item.enabled},#{item.mobilePhone},#{item.wrongPasswordCount},#{item.isDeleted})   </foreach>  </insert>  <select id='selectSysUsers' resultMap='SysUserResultMap'>    select * from sys_user  </select>  <select id='selectSysUserBySql' parameterType='java.lang.String' resultMap='SysUserResultMap'>    ${value}  </select>  <delete id='deleteSysUserBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysUserBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysUserByCondition' parameterType='java.lang.String' resultMap='SysUserResultMap'>    select * from sys_user where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_user where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>