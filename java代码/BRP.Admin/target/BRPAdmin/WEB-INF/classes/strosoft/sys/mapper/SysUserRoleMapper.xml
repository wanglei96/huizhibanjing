<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysUserRole'>  <resultMap type='strosoft.sys.data.SysUserRole' id='SysUserRoleResultMap' >    <result column='sys_user_id' property='sysUserId' />    <result column='sys_role_id' property='sysRoleId' />  </resultMap>  <insert id='insertSysUserRole' parameterType='strosoft.sys.data.SysUserRole'>    insert into sys_user_role    (sys_user_id,sys_role_id)    values    (#{sysUserId},#{sysRoleId})  </insert>  <select id='selectSysUserRole' parameterType='strosoft.sys.data.SysUserRole' resultMap='SysUserRoleResultMap'>    select * from sys_user_role where sys_user_id=#{sysUserId} and sys_role_id=#{sysRoleId}  </select>  <update id='updateSysUserRole' parameterType='strosoft.sys.data.SysUserRole'>    update sys_user_role  <trim prefix="set" suffixOverrides=",">    <if test="sysUserId !=null">sys_user_id=#{sysUserId},</if>   <if test="sysRoleId !=null">sys_role_id=#{sysRoleId},</if>   </trim>   <where>    sys_user_id=#{sysUserId} and sys_role_id=#{sysRoleId}  </where>  </update>  <delete id='deleteSysUserRole' parameterType='strosoft.sys.data.SysUserRole'>    delete from sys_user_role where sys_user_id=#{sysUserId} and sys_role_id=#{sysRoleId}  </delete>  <select id='selectSysUserRoleBySysRoleId' parameterType='java.lang.Integer' resultMap='SysUserRoleResultMap'>    select * from sys_user_role where sys_role_id=#{sysRoleId}  </select>  <select id='selectSysUserRoleBySysUserId' parameterType='java.lang.Integer' resultMap='SysUserRoleResultMap'>    select * from sys_user_role where sys_user_id=#{sysUserId}  </select>  <insert id='insertSysUserRoles' parameterType='java.util.List'>    insert into sys_user_role    (sys_user_id,sys_role_id)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.sysUserId},#{item.sysRoleId})   </foreach>  </insert>  <select id='selectSysUserRoles' resultMap='SysUserRoleResultMap'>    select * from sys_user_role  </select>  <select id='selectSysUserRoleBySql' parameterType='java.lang.String' resultMap='SysUserRoleResultMap'>    ${value}  </select>  <delete id='deleteSysUserRoleBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysUserRoleBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysUserRoleByCondition' parameterType='java.lang.String' resultMap='SysUserRoleResultMap'>    select * from sys_user_role where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_user_role where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>