<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysBasicTableRelation'>  <resultMap type='strosoft.sys.data.SysBasicTableRelation' id='SysBasicTableRelationResultMap' >    <result column='related_table_name' property='relatedTableName' />    <result column='related_column_name' property='relatedColumnName' />    <result column='basic_table_name' property='basicTableName' />  </resultMap>  <insert id='insertSysBasicTableRelation' parameterType='strosoft.sys.data.SysBasicTableRelation'>    insert into sys_basic_table_relation    (related_table_name,related_column_name,basic_table_name)    values    (#{relatedTableName},#{relatedColumnName},#{basicTableName})  </insert>  <select id='selectSysBasicTableRelation' parameterType='strosoft.sys.data.SysBasicTableRelation' resultMap='SysBasicTableRelationResultMap'>    select * from sys_basic_table_relation where related_table_name=#{relatedTableName} and related_column_name=#{relatedColumnName}  </select>  <update id='updateSysBasicTableRelation' parameterType='strosoft.sys.data.SysBasicTableRelation'>    update sys_basic_table_relation  <trim prefix="set" suffixOverrides=",">    <if test="relatedTableName !=null">related_table_name=#{relatedTableName},</if>   <if test="relatedColumnName !=null">related_column_name=#{relatedColumnName},</if>   <if test="basicTableName !=null">basic_table_name=#{basicTableName},</if>   </trim>   <where>    related_table_name=#{relatedTableName} and related_column_name=#{relatedColumnName}  </where>  </update>  <delete id='deleteSysBasicTableRelation' parameterType='strosoft.sys.data.SysBasicTableRelation'>    delete from sys_basic_table_relation where related_table_name=#{relatedTableName} and related_column_name=#{relatedColumnName}  </delete>  <select id='selectSysBasicTableRelationByBasicTableName' parameterType='java.lang.String' resultMap='SysBasicTableRelationResultMap'>    select * from sys_basic_table_relation where basic_table_name=#{basicTableName}  </select>  <insert id='insertSysBasicTableRelations' parameterType='java.util.List'>    insert into sys_basic_table_relation    (related_table_name,related_column_name,basic_table_name)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.relatedTableName},#{item.relatedColumnName},#{item.basicTableName})   </foreach>  </insert>  <select id='selectSysBasicTableRelations' resultMap='SysBasicTableRelationResultMap'>    select * from sys_basic_table_relation  </select>  <select id='selectSysBasicTableRelationBySql' parameterType='java.lang.String' resultMap='SysBasicTableRelationResultMap'>    ${value}  </select>  <delete id='deleteSysBasicTableRelationBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysBasicTableRelationBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysBasicTableRelationByCondition' parameterType='java.lang.String' resultMap='SysBasicTableRelationResultMap'>    select * from sys_basic_table_relation where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_basic_table_relation where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>