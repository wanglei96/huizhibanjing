<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysTotalType'>  <resultMap type='strosoft.sys.data.SysTotalType' id='SysTotalTypeResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertSysTotalType' parameterType='strosoft.sys.data.SysTotalType' keyProperty='id' useGeneratedKeys='true'>    insert into sys_total_type    (id,name,description,display_order)    values    (#{id},#{name},#{description},#{displayOrder})  </insert>  <select id='selectSysTotalType' parameterType='java.lang.Integer' resultMap='SysTotalTypeResultMap'>    select * from sys_total_type where id=#{id}  </select>  <update id='updateSysTotalType' parameterType='strosoft.sys.data.SysTotalType'>    update sys_total_type set id=#{id},name=#{name},description=#{description},display_order=#{displayOrder}    where id=#{id}  </update>  <delete id='deleteSysTotalType' parameterType='java.lang.Integer'>    delete from sys_total_type where id=#{id}  </delete>  <insert id='insertSysTotalTypes' parameterType='java.util.List'>    insert into sys_total_type    (id,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectSysTotalTypes' resultMap='SysTotalTypeResultMap'>    select * from sys_total_type  </select>  <select id='selectSysTotalTypeBySql' parameterType='java.lang.String' resultMap='SysTotalTypeResultMap'>    ${value}  </select>  <delete id='deleteSysTotalTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysTotalTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysTotalTypeByCondition' parameterType='java.lang.String' resultMap='SysTotalTypeResultMap'>    select * from sys_total_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_total_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>