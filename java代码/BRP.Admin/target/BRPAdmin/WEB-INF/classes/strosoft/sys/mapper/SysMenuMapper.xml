<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysMenu'>  <resultMap type='strosoft.sys.data.SysMenu' id='SysMenuResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='value' property='value' />    <result column='parent_id' property='parentId' />    <result column='url' property='url' />    <result column='display_order' property='displayOrder' />    <result column='sys_menu_type_code' property='sysMenuTypeCode' />    <result column='show_in_dashboard' property='showInDashboard' />    <result column='picture_id' property='pictureId' />    <result column='activation_picture_id' property='activationPictureId' />    <result column='icon' property='icon' />    <result column='path' property='path' />  </resultMap>  <insert id='insertSysMenu' parameterType='strosoft.sys.data.SysMenu' keyProperty='id' useGeneratedKeys='true'>    insert into sys_menu    (id,name,value,parent_id,url,display_order,sys_menu_type_code,show_in_dashboard,picture_id,activation_picture_id,icon,path)    values    (#{id},#{name},#{value},#{parentId},#{url},#{displayOrder},#{sysMenuTypeCode},#{showInDashboard},#{pictureId},#{activationPictureId},#{icon},#{path})  </insert>  <select id='selectSysMenu' parameterType='java.lang.Integer' resultMap='SysMenuResultMap'>    select * from sys_menu where id=#{id}  </select>  <update id='updateSysMenu' parameterType='strosoft.sys.data.SysMenu'>    update sys_menu set id=#{id},name=#{name},value=#{value},parent_id=#{parentId},url=#{url},display_order=#{displayOrder},sys_menu_type_code=#{sysMenuTypeCode},show_in_dashboard=#{showInDashboard},picture_id=#{pictureId},activation_picture_id=#{activationPictureId},icon=#{icon},path=#{path}    where id=#{id}  </update>  <delete id='deleteSysMenu' parameterType='java.lang.Integer'>    delete from sys_menu where id=#{id}  </delete>  <select id='selectSysMenuByActivationPictureId' parameterType='java.lang.Integer' resultMap='SysMenuResultMap'>    select * from sys_menu where activation_picture_id=#{activationPictureId}  </select>  <select id='selectSysMenuByPictureId' parameterType='java.lang.Integer' resultMap='SysMenuResultMap'>    select * from sys_menu where picture_id=#{pictureId}  </select>  <select id='selectSysMenuBySysMenuTypeCode' parameterType='java.lang.String' resultMap='SysMenuResultMap'>    select * from sys_menu where sys_menu_type_code=#{sysMenuTypeCode}  </select>  <delete id='deleteSysMenuByActivationPictureId' parameterType='java.lang.Integer'>    delete from sys_menu where activation_picture_id=#{activationPictureId}  </delete>  <delete id='deleteSysMenuByPictureId' parameterType='java.lang.Integer'>    delete from sys_menu where picture_id=#{pictureId}  </delete>  <delete id='deleteSysMenuBySysMenuTypeCode' parameterType='java.lang.String'>    delete from sys_menu where sys_menu_type_code=#{sysMenuTypeCode}  </delete>  <insert id='insertSysMenus' parameterType='java.util.List'>    insert into sys_menu    (id,name,value,parent_id,url,display_order,sys_menu_type_code,show_in_dashboard,picture_id,activation_picture_id,icon,path)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.value},#{item.parentId},#{item.url},#{item.displayOrder},#{item.sysMenuTypeCode},#{item.showInDashboard},#{item.pictureId},#{item.activationPictureId},#{item.icon},#{item.path})   </foreach>  </insert>  <select id='selectSysMenus' resultMap='SysMenuResultMap'>    select * from sys_menu  </select>  <select id='selectSysMenuBySql' parameterType='java.lang.String' resultMap='SysMenuResultMap'>    ${value}  </select>  <delete id='deleteSysMenuBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysMenuBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysMenuByCondition' parameterType='java.lang.String' resultMap='SysMenuResultMap'>    select * from sys_menu where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_menu where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>