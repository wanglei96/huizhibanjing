<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysSettingCategory'>  <resultMap type='strosoft.sys.data.SysSettingCategory' id='SysSettingCategoryResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='parent_code' property='parentCode' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertSysSettingCategory' parameterType='strosoft.sys.data.SysSettingCategory'>    insert into sys_setting_category    (code,name,description,display_order,parent_code,create_time,update_time)    values    (#{code},#{name},#{description},#{displayOrder},#{parentCode},now(),now())  </insert>  <select id='selectSysSettingCategory' parameterType='java.lang.String' resultMap='SysSettingCategoryResultMap'>    select * from sys_setting_category where code=#{code}  </select>  <update id='updateSysSettingCategory' parameterType='strosoft.sys.data.SysSettingCategory'>    update sys_setting_category  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   <if test="parentCode !=null">parent_code=#{parentCode},</if>   <if test="createTime !=null">create_time=#{createTime},</if>   <if test="updateTime !=null">update_time=#{updateTime},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysSettingCategory' parameterType='java.lang.String'>    delete from sys_setting_category where code=#{code}  </delete>  <insert id='insertSysSettingCategorys' parameterType='java.util.List'>    insert into sys_setting_category    (code,name,description,display_order,parent_code,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder},#{item.parentCode},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectSysSettingCategorys' resultMap='SysSettingCategoryResultMap'>    select * from sys_setting_category  </select>  <select id='selectSysSettingCategoryBySql' parameterType='java.lang.String' resultMap='SysSettingCategoryResultMap'>    ${value}  </select>  <delete id='deleteSysSettingCategoryBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysSettingCategoryBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysSettingCategoryByCondition' parameterType='java.lang.String' resultMap='SysSettingCategoryResultMap'>    select * from sys_setting_category where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_setting_category where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>