<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysBasicTable'>  <resultMap type='strosoft.sys.data.SysBasicTable' id='SysBasicTableResultMap' >    <result column='id' property='id' />    <result column='table_name' property='tableName' />    <result column='title' property='title' />    <result column='sys_basic_table_type_code' property='sysBasicTableTypeCode' />    <result column='description' property='description' />  </resultMap>  <insert id='insertSysBasicTable' parameterType='strosoft.sys.data.SysBasicTable' keyProperty='id' useGeneratedKeys='true'>    insert into sys_basic_table    (id,table_name,title,sys_basic_table_type_code,description)    values    (#{id},#{tableName},#{title},#{sysBasicTableTypeCode},#{description})  </insert>  <select id='selectSysBasicTable' parameterType='java.lang.Integer' resultMap='SysBasicTableResultMap'>    select * from sys_basic_table where id=#{id}  </select>  <update id='updateSysBasicTable' parameterType='strosoft.sys.data.SysBasicTable'>    update sys_basic_table set id=#{id},table_name=#{tableName},title=#{title},sys_basic_table_type_code=#{sysBasicTableTypeCode},description=#{description}    where id=#{id}  </update>  <delete id='deleteSysBasicTable' parameterType='java.lang.Integer'>    delete from sys_basic_table where id=#{id}  </delete>  <select id='selectSysBasicTableBySysBasicTableTypeCode' parameterType='java.lang.String' resultMap='SysBasicTableResultMap'>    select * from sys_basic_table where sys_basic_table_type_code=#{sysBasicTableTypeCode}  </select>  <delete id='deleteSysBasicTableBySysBasicTableTypeCode' parameterType='java.lang.String'>    delete from sys_basic_table where sys_basic_table_type_code=#{sysBasicTableTypeCode}  </delete>  <insert id='insertSysBasicTables' parameterType='java.util.List'>    insert into sys_basic_table    (id,table_name,title,sys_basic_table_type_code,description)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.tableName},#{item.title},#{item.sysBasicTableTypeCode},#{item.description})   </foreach>  </insert>  <select id='selectSysBasicTables' resultMap='SysBasicTableResultMap'>    select * from sys_basic_table  </select>  <select id='selectSysBasicTableBySql' parameterType='java.lang.String' resultMap='SysBasicTableResultMap'>    ${value}  </select>  <delete id='deleteSysBasicTableBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysBasicTableBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysBasicTableByCondition' parameterType='java.lang.String' resultMap='SysBasicTableResultMap'>    select * from sys_basic_table where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_basic_table where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>