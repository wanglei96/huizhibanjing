<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysUploadFile'>  <resultMap type='strosoft.sys.data.SysUploadFile' id='SysUploadFileResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='path' property='path' />    <result column='small_image_file_path' property='smallImageFilePath' />    <result column='time_image_file_path' property='timeImageFilePath' />    <result column='video_image_file_path' property='videoImageFilePath' />    <result column='create_time' property='createTime' />  </resultMap>  <insert id='insertSysUploadFile' parameterType='strosoft.sys.data.SysUploadFile' keyProperty='id' useGeneratedKeys='true'>    insert into sys_upload_file    (id,name,path,small_image_file_path,time_image_file_path,video_image_file_path,create_time)    values    (#{id},#{name},#{path},#{smallImageFilePath},#{timeImageFilePath},#{videoImageFilePath},now())  </insert>  <select id='selectSysUploadFile' parameterType='java.lang.Integer' resultMap='SysUploadFileResultMap'>    select * from sys_upload_file where id=#{id}  </select>  <update id='updateSysUploadFile' parameterType='strosoft.sys.data.SysUploadFile'>    update sys_upload_file set id=#{id},name=#{name},path=#{path},small_image_file_path=#{smallImageFilePath},time_image_file_path=#{timeImageFilePath},video_image_file_path=#{videoImageFilePath},create_time=#{createTime}    where id=#{id}  </update>  <delete id='deleteSysUploadFile' parameterType='java.lang.Integer'>    delete from sys_upload_file where id=#{id}  </delete>  <insert id='insertSysUploadFiles' parameterType='java.util.List'>    insert into sys_upload_file    (id,name,path,small_image_file_path,time_image_file_path,video_image_file_path,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.path},#{item.smallImageFilePath},#{item.timeImageFilePath},#{item.videoImageFilePath},now())   </foreach>  </insert>  <select id='selectSysUploadFiles' resultMap='SysUploadFileResultMap'>    select * from sys_upload_file  </select>  <select id='selectSysUploadFileBySql' parameterType='java.lang.String' resultMap='SysUploadFileResultMap'>    ${value}  </select>  <delete id='deleteSysUploadFileBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysUploadFileBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysUploadFileByCondition' parameterType='java.lang.String' resultMap='SysUploadFileResultMap'>    select * from sys_upload_file where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_upload_file where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>