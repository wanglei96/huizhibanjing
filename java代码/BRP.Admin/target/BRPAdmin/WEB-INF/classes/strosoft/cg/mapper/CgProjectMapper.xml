<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CgProject'>	<resultMap type='strosoft.cg.data.CgProject'		id='CgProjectResultMap'>		<result column='id' property='id' />		<result column='name' property='name' />		<result column='configuration' property='configuration' />		<result column='database_address' property='databaseAddress' />		<result column='database_port' property='databasePort' />		<result column='database_name' property='databaseName' />		<result column='database_username' property='databaseUsername' />		<result column='database_password' property='databasePassword' />		<result column='output_directory' property='outputDirectory' />		<result column='create_time' property='createTime' />		<result column='update_time' property='updateTime' />	</resultMap>	<insert id='insertCgProject'		parameterType='strosoft.cg.data.CgProject' keyProperty='id'		useGeneratedKeys='true'>		insert into cg_project		(id,name,configuration,database_address,database_port,database_name,database_username,database_password,output_directory,create_time,update_time)		values		(#{id},#{name},#{configuration},#{databaseAddress},#{databasePort},#{databaseName},#{databaseUsername},#{databasePassword},#{outputDirectory},now(),#{updateTime})	</insert>	<select id='selectCgProject' parameterType='java.lang.Integer'		resultMap='CgProjectResultMap'>		select * from cg_project where id=#{id}	</select>	<update id='updateCgProject'		parameterType='strosoft.cg.data.CgProject'>		update cg_project set		id=#{id},name=#{name},configuration=#{configuration},database_address=#{databaseAddress},database_port=#{databasePort},database_name=#{databaseName},database_username=#{databaseUsername},database_password=#{databasePassword},output_directory=#{outputDirectory},create_time=#{createTime},update_time=#{updateTime}		where id=#{id}	</update>	<delete id='deleteCgProject' parameterType='java.lang.Integer'>		delete from cg_project where id=#{id}	</delete>	<insert id='insertCgProjects' parameterType='java.util.List'>		insert into cg_project		(id,name,configuration,database_address,database_port,database_name,database_username,database_password,output_directory,create_time,update_time)		values		<foreach collection='list' item='item' index='index'			separator=','>			(#{item.id},#{item.name},#{item.configuration},#{item.databaseAddress},#{item.databasePort},#{item.databaseName},#{item.databaseUsername},#{item.databasePassword},#{item.outputDirectory},now(),#{item.updateTime})		</foreach>	</insert>	<select id='selectCgProjects' resultMap='CgProjectResultMap'>		select * from cg_project	</select>	<select id='selectCgProjectBySql'		parameterType='java.lang.String' resultMap='CgProjectResultMap'>		${value}	</select>	<delete id='deleteCgProjectBySql'		parameterType='java.lang.String'>		${value}	</delete>	<update id='updateCgProjectBySql'		parameterType='java.lang.String'>		${value}	</update>	<select id='selectCgProjectByCondition'		parameterType='java.lang.String' resultMap='CgProjectResultMap'>		select * from cg_project where ${value}	</select>	<select id='selectCountByCondition'		parameterType='java.lang.String' resultType='java.lang.Integer'>		select count(*) from cg_project where ${value}	</select>	<select id='selectIntBySql' parameterType='java.lang.String'		resultType='java.lang.Integer'>		${value}	</select>	<!-- 查询 -->	<insert id="insertBysql" parameterType="java.lang.String">		${value}	</insert>	<select id="selectStringBysql" parameterType="java.lang.String"		resultType="java.lang.String">		${value}	</select>	<update id="updateBysql" parameterType="java.lang.String">		${value}	</update>	<delete id="deleteStringBysql" parameterType="java.lang.String">		${value}	</delete></mapper>