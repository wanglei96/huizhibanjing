<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="strosoft.app.common">
	<select id="executeList" resultType="java.util.LinkedHashMap"
		parameterType="JSONObject">
		${sql};
	</select>
	<insert id="executeAdd" parameterType="java.lang.String">
		${sql};
	</insert>
	<update id="executeUpdate" parameterType="JSONObject">
		${sql};
	</update>
	<delete id="executeDelete" parameterType="JSONObject">
		${sql};
	</delete>
	<select id="executeInt" resultType="java.lang.Integer"
		parameterType="JSONObject">
		${sql};
	</select>
	<select id="executeString" resultType="java.lang.String"
		parameterType="JSONObject">
		${sql};
	</select>
	<select id="selectStringBysql" resultType="java.lang.String"
		parameterType="java.lang.String">
		${value};
	</select>
</mapper>
