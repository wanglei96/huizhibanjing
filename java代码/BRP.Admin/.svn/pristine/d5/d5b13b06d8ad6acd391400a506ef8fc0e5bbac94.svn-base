<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysFunction'>  <resultMap type='strosoft.sys.data.SysFunction' id='SysFunctionResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='resource' property='resource' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertSysFunction' parameterType='strosoft.sys.data.SysFunction'>    insert into sys_function    (code,name,resource,description,display_order)    values    (#{code},#{name},#{resource},#{description},#{displayOrder})  </insert>  <select id='selectSysFunction' parameterType='java.lang.String' resultMap='SysFunctionResultMap'>    select * from sys_function where code=#{code}  </select>  <update id='updateSysFunction' parameterType='strosoft.sys.data.SysFunction'>    update sys_function  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="resource !=null">resource=#{resource},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysFunction' parameterType='java.lang.String'>    delete from sys_function where code=#{code}  </delete>  <select id='selectSysFunctionByResource' parameterType='java.lang.String' resultMap='SysFunctionResultMap'>    select * from sys_function where resource=#{resource}  </select>  <delete id='deleteSysFunctionByResource' parameterType='java.lang.String'>    delete from sys_function where resource=#{resource}  </delete>  <insert id='insertSysFunctions' parameterType='java.util.List'>    insert into sys_function    (code,name,resource,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.resource},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectSysFunctions' resultMap='SysFunctionResultMap'>    select * from sys_function  </select>  <select id='selectSysFunctionBySql' parameterType='java.lang.String' resultMap='SysFunctionResultMap'>    ${value}  </select>  <delete id='deleteSysFunctionBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysFunctionBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysFunctionByCondition' parameterType='java.lang.String' resultMap='SysFunctionResultMap'>    select * from sys_function where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_function where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>