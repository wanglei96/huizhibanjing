        }
        return flag;
    }
    public  boolean existsId(SqlSession sqlSession,Integer id)    
    {
        Integer rows = sqlSession.selectOne("Book.selectIntBySql","select count(*) from book where id= '"+id+"'");
        return rows>0?true:false;
    }
