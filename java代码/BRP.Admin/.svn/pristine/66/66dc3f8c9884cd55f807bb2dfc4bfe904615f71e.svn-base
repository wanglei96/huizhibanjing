<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='MemberBorrowTotal'>  <resultMap type='BRP.data.MemberBorrowTotal' id='MemberBorrowTotalResultMap' >    <result column='member_id' property='memberId' />    <result column='company_id' property='companyId' />    <result column='member_name' property='memberName' />    <result column='mobile_phone' property='mobilePhone' />    <result column='total_borrow_count' property='totalBorrowCount' />    <result column='total_price' property='totalPrice' />    <result column='book_order_create_time' property='bookOrderCreateTime' />    <result column='card_no' property='cardNo' />    <result column='batch_name' property='batchName' />    <result column='expire_date' property='expireDate' />    <result column='effective_date' property='effectiveDate' />  </resultMap>  <select id='selectMemberBorrowTotals' resultMap='MemberBorrowTotalResultMap'>    select * from member_borrow_total  </select>  <select id='selectMemberBorrowTotalBySql' parameterType='java.lang.String' resultMap='MemberBorrowTotalResultMap'>    ${value}  </select>  <select id='selectMemberBorrowTotalByCondition' parameterType='java.lang.String' resultMap='MemberBorrowTotalResultMap'>    select * from member_borrow_total where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from member_borrow_total where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>