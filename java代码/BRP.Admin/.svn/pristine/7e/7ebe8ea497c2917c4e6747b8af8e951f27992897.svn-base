package strosoft.cms.data;

import java.util.HashMap;

public class CmsPageItemsContext {
	private String autoHide; //是否在无分页时不显示标签内容。
	public CmsPageItemsContext(){
		
	}
	public CmsPageItemsContext(HashMap<String, String> attributeCondition) {
		this.autoHide = attributeCondition.get("autoHide".toLowerCase());
		
	}
	public String getAutoHide() {
		return autoHide;
	}
	public void setAutoHide(String autoHide) {
		this.autoHide = autoHide;
	}
	
}
