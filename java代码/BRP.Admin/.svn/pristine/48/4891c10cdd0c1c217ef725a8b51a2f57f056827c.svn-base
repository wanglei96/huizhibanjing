    public  int delete(SqlSession sqlSession,DataEntity entity)    
    {
        BookCompensationManager.getInstance().deleteByPaymentMethodCode(sqlSession,((PaymentMethod)entity).getCode());
        BookDamageManager.getInstance().deleteByPaymentMethodCode(sqlSession,((PaymentMethod)entity).getCode());
        MemberManager.getInstance().deleteByPaymentMethodCode(sqlSession,((PaymentMethod)entity).getCode());
        MemberCardManager.getInstance().deleteByPaymentMethodCode(sqlSession,((PaymentMethod)entity).getCode());
        MemberCardServiceTimeLogManager.getInstance().deleteByPaymentMethodCode(sqlSession,((PaymentMethod)entity).getCode());
        PayOrderManager.getInstance().deleteByPaymentMethodCode(sqlSession,((PaymentMethod)entity).getCode());
        SalesOrderManager.getInstance().deleteByPaymentMethodCode(sqlSession,((PaymentMethod)entity).getCode());
        int rows = sqlSession.delete("PaymentMethod.deletePaymentMethod",entity);
        return rows;
    }
    public  int batchDelete(List<String> list)throws Exception    
    {
        SqlSession sqlSession = MyBatisManager.getInstance().openSession();
        int rows = 0;
        try        
        {
            for(int i=0;i<list.size();i++)            
            {
                rows += delete(sqlSession,list.get(i));
            }
            sqlSession.commit();
        }
        catch(Exception  e)        
        {
            e.printStackTrace();
            sqlSession.rollback();
            rows=0;
            throw e;
        }
         finally         
        {
            sqlSession.close();
        }
        return rows;
    }
