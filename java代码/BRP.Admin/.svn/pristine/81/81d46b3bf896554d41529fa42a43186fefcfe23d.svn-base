<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='HomeRandomBook'>  <resultMap type='BRP.data.HomeRandomBook' id='HomeRandomBookResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='book_sku_id' property='bookSkuId' />    <result column='age_group_id' property='ageGroupId' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertHomeRandomBook' parameterType='BRP.data.HomeRandomBook' keyProperty='id' useGeneratedKeys='true'>    insert into home_random_book    (id,company_id,book_sku_id,age_group_id,create_time,update_time)    values    (#{id},#{companyId},#{bookSkuId},#{ageGroupId},now(),now())  </insert>  <select id='selectHomeRandomBook' parameterType='java.lang.Integer' resultMap='HomeRandomBookResultMap'>    select * from home_random_book where id=#{id}  </select>  <update id='updateHomeRandomBook' parameterType='BRP.data.HomeRandomBook'>    update home_random_book set id=#{id},company_id=#{companyId},book_sku_id=#{bookSkuId},age_group_id=#{ageGroupId},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteHomeRandomBook' parameterType='java.lang.Integer'>    delete from home_random_book where id=#{id}  </delete>  <select id='selectHomeRandomBookByBookSkuId' parameterType='java.lang.Integer' resultMap='HomeRandomBookResultMap'>    select * from home_random_book where book_sku_id=#{bookSkuId}  </select>  <select id='selectHomeRandomBookByCompanyId' parameterType='java.lang.Integer' resultMap='HomeRandomBookResultMap'>    select * from home_random_book where company_id=#{companyId}  </select>  <delete id='deleteHomeRandomBookByBookSkuId' parameterType='java.lang.Integer'>    delete from home_random_book where book_sku_id=#{bookSkuId}  </delete>  <delete id='deleteHomeRandomBookByCompanyId' parameterType='java.lang.Integer'>    delete from home_random_book where company_id=#{companyId}  </delete>  <insert id='insertHomeRandomBooks' parameterType='java.util.List'>    insert into home_random_book    (id,company_id,book_sku_id,age_group_id,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.bookSkuId},#{item.ageGroupId},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectHomeRandomBooks' resultMap='HomeRandomBookResultMap'>    select * from home_random_book  </select>  <select id='selectHomeRandomBookBySql' parameterType='java.lang.String' resultMap='HomeRandomBookResultMap'>    ${value}  </select>  <delete id='deleteHomeRandomBookBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateHomeRandomBookBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectHomeRandomBookByCondition' parameterType='java.lang.String' resultMap='HomeRandomBookResultMap'>    select * from home_random_book where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from home_random_book where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>