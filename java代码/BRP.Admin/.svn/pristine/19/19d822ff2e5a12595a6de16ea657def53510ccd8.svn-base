package strosoft.cms.data;

import java.util.HashMap;

public class CmsLocationContext {
		
	private String isDynamic;
	private String linkClass;
	private String separator;
	private String target;
	private String wordNum;
	
	public CmsLocationContext() {
		// TODO Auto-generated constructor stub
	}
	public CmsLocationContext(HashMap<String, String> attributeCondition) {
		// TODO Auto-generated constructor stub
		this.isDynamic=attributeCondition.get("isDynamic".toLowerCase());
		this.linkClass=attributeCondition.get("linkClass".toLowerCase());
		this.separator=attributeCondition.get("separator".toLowerCase());
		this.target=attributeCondition.get("target".toLowerCase());
		this.wordNum=attributeCondition.get("wordNum".toLowerCase());
	}
	
	public String getIsDynamic() {
		return isDynamic;
	}
	public void setIsDynamic(String isDynamic) {
		this.isDynamic = isDynamic;
	}
	public String getLinkClass() {
		return linkClass;
	}
	public void setLinkClass(String linkClass) {
		this.linkClass = linkClass;
	}
	public String getSeparator() {
		return separator;
	}
	public void setSeparator(String separator) {
		this.separator = separator;
	}
	public String getTarget() {
		return target;
	}
	public void setTarget(String target) {
		this.target = target;
	}
	public String getWordNum() {
		return wordNum;
	}
	public void setWordNum(String wordNum) {
		this.wordNum = wordNum;
	}
	
	
	
}
