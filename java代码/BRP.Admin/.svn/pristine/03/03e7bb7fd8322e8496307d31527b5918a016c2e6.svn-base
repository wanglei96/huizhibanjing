<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewReadActivity'>  <resultMap type='BRP.data.ViewReadActivity' id='ViewReadActivityResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='image_file_id' property='imageFileId' />    <result column='price' property='price' />    <result column='content' property='content' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />    <result column='image_file_path' property='imageFilePath' />  </resultMap>  <select id='selectViewReadActivitys' resultMap='ViewReadActivityResultMap'>    select * from view_read_activity  </select>  <select id='selectViewReadActivityById' parameterType='java.lang.Integer' resultMap='ViewReadActivityResultMap'>    select * from view_read_activity where id= #{id}  </select>  <select id='selectViewReadActivityBySql' parameterType='java.lang.String' resultMap='ViewReadActivityResultMap'>    ${value}  </select>  <select id='selectViewReadActivityByCondition' parameterType='java.lang.String' resultMap='ViewReadActivityResultMap'>    select * from view_read_activity where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_read_activity where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>