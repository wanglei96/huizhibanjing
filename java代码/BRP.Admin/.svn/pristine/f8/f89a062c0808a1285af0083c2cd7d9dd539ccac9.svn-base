package strosoft.cms.data;

import java.util.HashMap;

public class CmsPageItemContext {
	private String alwaysA; // "页码总是超链接，包括无连接时"
	private String hasLR; // "页码导航是否包含左右字符"
	private String linkClass; // "链接CSS样式"
	private String listEllipsis; // "页导航或页跳转链接太多时显示的省略号"
	private String listNum; // "页导航或页跳转显示链接数"
	private String lStr; // "页面左字符"
	private String rStr; // "页面右字符"
	private String text; // "显示的文字"
	private String textClass; // "文字CSS样式"
	private String type; // "类型
	private String target;//链接跳转目的页面

	public CmsPageItemContext() {

	}
	
	public CmsPageItemContext(HashMap<String, String> attributeCondition) {
		 this.alwaysA = attributeCondition.get("alwaysA".toLowerCase());
		 this.hasLR = attributeCondition.get("hasLR".toLowerCase());
		 this.linkClass = attributeCondition.get("linkClass".toLowerCase());
		 this.listEllipsis = attributeCondition.get("listEllipsis".toLowerCase());
		 this.listNum = attributeCondition.get("listNum".toLowerCase());
		 this.lStr = attributeCondition.get("lStr".toLowerCase());
		 this.rStr = attributeCondition.get("rStr".toLowerCase());
		 this.text = attributeCondition.get("text".toLowerCase());
		 this.textClass = attributeCondition.get("textClass".toLowerCase());
		 this.type = attributeCondition.get("type".toLowerCase());
		 this.target = attributeCondition.get("target".toLowerCase());
	}

	public String getAlwaysA() {
		return alwaysA;
	}

	public void setAlwaysA(String alwaysA) {
		this.alwaysA = alwaysA;
	}

	public String getHasLR() {
		return hasLR;
	}

	public void setHasLR(String hasLR) {
		this.hasLR = hasLR;
	}

	public String getLinkClass() {
		return linkClass;
	}

	public void setLinkClass(String linkClass) {
		this.linkClass = linkClass;
	}

	public String getListEllipsis() {
		return listEllipsis;
	}

	public void setListEllipsis(String listEllipsis) {
		this.listEllipsis = listEllipsis;
	}

	public String getListNum() {
		return listNum;
	}

	public void setListNum(String listNum) {
		this.listNum = listNum;
	}

	public String getlStr() {
		return lStr;
	}

	public void setlStr(String lStr) {
		this.lStr = lStr;
	}

	public String getrStr() {
		return rStr;
	}

	public void setrStr(String rStr) {
		this.rStr = rStr;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

	public String getTextClass() {
		return textClass;
	}

	public void setTextClass(String textClass) {
		this.textClass = textClass;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getTarget() {
		return target;
	}

	public void setTarget(String target) {
		this.target = target;
	}

}
