<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanySettingValue'>  <resultMap type='BRP.data.CompanySettingValue' id='CompanySettingValueResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='company_setting_code' property='companySettingCode' />    <result column='value' property='value' />  </resultMap>  <insert id='insertCompanySettingValue' parameterType='BRP.data.CompanySettingValue' keyProperty='id' useGeneratedKeys='true'>    insert into company_setting_value    (id,company_id,company_setting_code,value)    values    (#{id},#{companyId},#{companySettingCode},#{value})  </insert>  <select id='selectCompanySettingValue' parameterType='java.lang.Integer' resultMap='CompanySettingValueResultMap'>    select * from company_setting_value where id=#{id}  </select>  <update id='updateCompanySettingValue' parameterType='BRP.data.CompanySettingValue'>    update company_setting_value set id=#{id},company_id=#{companyId},company_setting_code=#{companySettingCode},value=#{value}    where id=#{id}  </update>  <delete id='deleteCompanySettingValue' parameterType='java.lang.Integer'>    delete from company_setting_value where id=#{id}  </delete>  <insert id='insertCompanySettingValues' parameterType='java.util.List'>    insert into company_setting_value    (id,company_id,company_setting_code,value)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.companySettingCode},#{item.value})   </foreach>  </insert>  <select id='selectCompanySettingValues' resultMap='CompanySettingValueResultMap'>    select * from company_setting_value  </select>  <select id='selectCompanySettingValueBySql' parameterType='java.lang.String' resultMap='CompanySettingValueResultMap'>    ${value}  </select>  <delete id='deleteCompanySettingValueBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanySettingValueBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanySettingValueByCondition' parameterType='java.lang.String' resultMap='CompanySettingValueResultMap'>    select * from company_setting_value where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_setting_value where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>