<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsLanguage'>  <resultMap type='strosoft.cms.data.CmsLanguage' id='CmsLanguageResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='is_default' property='isDefault' />    <result column='cms_site_id' property='cmsSiteId' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='path' property='path' />  </resultMap>  <insert id='insertCmsLanguage' parameterType='strosoft.cms.data.CmsLanguage'>    insert into cms_language    (code,name,is_default,cms_site_id,description,display_order,path)    values    (#{code},#{name},#{isDefault},#{cmsSiteId},#{description},#{displayOrder},#{path})  </insert>  <select id='selectCmsLanguage' parameterType='java.lang.String' resultMap='CmsLanguageResultMap'>    select * from cms_language where code=#{code}  </select>  <update id='updateCmsLanguage' parameterType='strosoft.cms.data.CmsLanguage'>    update cms_language  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="isDefault !=null">is_default=#{isDefault},</if>   <if test="cmsSiteId !=null">cms_site_id=#{cmsSiteId},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   <if test="path !=null">path=#{path},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteCmsLanguage' parameterType='java.lang.String'>    delete from cms_language where code=#{code}  </delete>  <insert id='insertCmsLanguages' parameterType='java.util.List'>    insert into cms_language    (code,name,is_default,cms_site_id,description,display_order,path)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.isDefault},#{item.cmsSiteId},#{item.description},#{item.displayOrder},#{item.path})   </foreach>  </insert>  <select id='selectCmsLanguages' resultMap='CmsLanguageResultMap'>    select * from cms_language  </select>  <select id='selectCmsLanguageBySql' parameterType='java.lang.String' resultMap='CmsLanguageResultMap'>    ${value}  </select>  <delete id='deleteCmsLanguageBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsLanguageBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsLanguageByCondition' parameterType='java.lang.String' resultMap='CmsLanguageResultMap'>    select * from cms_language where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_language where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>