<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanyOrganization'>  <resultMap type='BRP.data.CompanyOrganization' id='CompanyOrganizationResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='display_order' property='displayOrder' />    <result column='description' property='description' />    <result column='create_time' property='createTime' />    <result column='company_id' property='companyId' />    <result column='parent_id' property='parentId' />    <result column='is_deleted' property='isDeleted' />  </resultMap>  <insert id='insertCompanyOrganization' parameterType='BRP.data.CompanyOrganization' keyProperty='id' useGeneratedKeys='true'>    insert into company_organization    (id,name,display_order,description,create_time,company_id,parent_id,is_deleted)    values    (#{id},#{name},#{displayOrder},#{description},now(),#{companyId},#{parentId},#{isDeleted})  </insert>  <select id='selectCompanyOrganization' parameterType='java.lang.Integer' resultMap='CompanyOrganizationResultMap'>    select * from company_organization where id=#{id}  </select>  <update id='updateCompanyOrganization' parameterType='BRP.data.CompanyOrganization'>    update company_organization set id=#{id},name=#{name},display_order=#{displayOrder},description=#{description},create_time=#{createTime},company_id=#{companyId},parent_id=#{parentId},is_deleted=#{isDeleted}    where id=#{id}  </update>  <delete id='deleteCompanyOrganization' parameterType='java.lang.Integer'>    delete from company_organization where id=#{id}  </delete>  <insert id='insertCompanyOrganizations' parameterType='java.util.List'>    insert into company_organization    (id,name,display_order,description,create_time,company_id,parent_id,is_deleted)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.displayOrder},#{item.description},now(),#{item.companyId},#{item.parentId},#{item.isDeleted})   </foreach>  </insert>  <select id='selectCompanyOrganizations' resultMap='CompanyOrganizationResultMap'>    select * from company_organization  </select>  <select id='selectCompanyOrganizationBySql' parameterType='java.lang.String' resultMap='CompanyOrganizationResultMap'>    ${value}  </select>  <delete id='deleteCompanyOrganizationBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanyOrganizationBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanyOrganizationByCondition' parameterType='java.lang.String' resultMap='CompanyOrganizationResultMap'>    select * from company_organization where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_organization where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>