<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='Deliver'>  <resultMap type='BRP.data.Deliver' id='DeliverResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='name' property='name' />    <result column='deliver_no' property='deliverNo' />    <result column='deliver_area_id' property='deliverAreaId' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertDeliver' parameterType='BRP.data.Deliver' keyProperty='id' useGeneratedKeys='true'>    insert into deliver    (id,company_id,name,deliver_no,deliver_area_id,create_time,update_time)    values    (#{id},#{companyId},#{name},#{deliverNo},#{deliverAreaId},now(),#{updateTime})  </insert>  <select id='selectDeliver' parameterType='java.lang.Integer' resultMap='DeliverResultMap'>    select * from deliver where id=#{id}  </select>  <update id='updateDeliver' parameterType='BRP.data.Deliver'>    update deliver set id=#{id},company_id=#{companyId},name=#{name},deliver_no=#{deliverNo},deliver_area_id=#{deliverAreaId},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteDeliver' parameterType='java.lang.Integer'>    delete from deliver where id=#{id}  </delete>  <select id='selectDeliverByCompanyId' parameterType='java.lang.Integer' resultMap='DeliverResultMap'>    select * from deliver where company_id=#{companyId}  </select>  <select id='selectDeliverByDeliverAreaId' parameterType='java.lang.Integer' resultMap='DeliverResultMap'>    select * from deliver where deliver_area_id=#{deliverAreaId}  </select>  <delete id='deleteDeliverByCompanyId' parameterType='java.lang.Integer'>    delete from deliver where company_id=#{companyId}  </delete>  <delete id='deleteDeliverByDeliverAreaId' parameterType='java.lang.Integer'>    delete from deliver where deliver_area_id=#{deliverAreaId}  </delete>  <insert id='insertDelivers' parameterType='java.util.List'>    insert into deliver    (id,company_id,name,deliver_no,deliver_area_id,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.name},#{item.deliverNo},#{item.deliverAreaId},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectDelivers' resultMap='DeliverResultMap'>    select * from deliver  </select>  <select id='selectDeliverBySql' parameterType='java.lang.String' resultMap='DeliverResultMap'>    ${value}  </select>  <delete id='deleteDeliverBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateDeliverBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectDeliverByCondition' parameterType='java.lang.String' resultMap='DeliverResultMap'>    select * from deliver where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from deliver where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>