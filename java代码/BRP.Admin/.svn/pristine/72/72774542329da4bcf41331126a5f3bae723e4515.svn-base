<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='Activity'>  <resultMap type='BRP.data.Activity' id='ActivityResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='name' property='name' />    <result column='activity_type_code' property='activityTypeCode' />    <result column='renewal_price' property='renewalPrice' />    <result column='buy_days' property='buyDays' />    <result column='gift_days' property='giftDays' />    <result column='loanable_num' property='loanableNum' />    <result column='gift_score' property='giftScore' />    <result column='gift_stop_card_quantity' property='giftStopCardQuantity' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertActivity' parameterType='BRP.data.Activity' keyProperty='id' useGeneratedKeys='true'>    insert into activity    (id,company_id,name,activity_type_code,renewal_price,buy_days,gift_days,loanable_num,gift_score,gift_stop_card_quantity,create_time,update_time)    values    (#{id},#{companyId},#{name},#{activityTypeCode},#{renewalPrice},#{buyDays},#{giftDays},#{loanableNum},#{giftScore},#{giftStopCardQuantity},now(),now())  </insert>  <select id='selectActivity' parameterType='java.lang.Integer' resultMap='ActivityResultMap'>    select * from activity where id=#{id}  </select>  <update id='updateActivity' parameterType='BRP.data.Activity'>    update activity set id=#{id},company_id=#{companyId},name=#{name},activity_type_code=#{activityTypeCode},renewal_price=#{renewalPrice},buy_days=#{buyDays},gift_days=#{giftDays},loanable_num=#{loanableNum},gift_score=#{giftScore},gift_stop_card_quantity=#{giftStopCardQuantity},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteActivity' parameterType='java.lang.Integer'>    delete from activity where id=#{id}  </delete>  <select id='selectActivityByActivityTypeCode' parameterType='java.lang.String' resultMap='ActivityResultMap'>    select * from activity where activity_type_code=#{activityTypeCode}  </select>  <select id='selectActivityByCompanyId' parameterType='java.lang.Integer' resultMap='ActivityResultMap'>    select * from activity where company_id=#{companyId}  </select>  <delete id='deleteActivityByActivityTypeCode' parameterType='java.lang.String'>    delete from activity where activity_type_code=#{activityTypeCode}  </delete>  <delete id='deleteActivityByCompanyId' parameterType='java.lang.Integer'>    delete from activity where company_id=#{companyId}  </delete>  <insert id='insertActivitys' parameterType='java.util.List'>    insert into activity    (id,company_id,name,activity_type_code,renewal_price,buy_days,gift_days,loanable_num,gift_score,gift_stop_card_quantity,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.name},#{item.activityTypeCode},#{item.renewalPrice},#{item.buyDays},#{item.giftDays},#{item.loanableNum},#{item.giftScore},#{item.giftStopCardQuantity},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectActivitys' resultMap='ActivityResultMap'>    select * from activity  </select>  <select id='selectActivityBySql' parameterType='java.lang.String' resultMap='ActivityResultMap'>    ${value}  </select>  <delete id='deleteActivityBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateActivityBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectActivityByCondition' parameterType='java.lang.String' resultMap='ActivityResultMap'>    select * from activity where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from activity where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>