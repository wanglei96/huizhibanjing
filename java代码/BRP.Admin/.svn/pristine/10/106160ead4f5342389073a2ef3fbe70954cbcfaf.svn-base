<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='MessageTemplate'>  <resultMap type='BRP.data.MessageTemplate' id='MessageTemplateResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='pusher_code' property='pusherCode' />    <result column='push_time' property='pushTime' />    <result column='message_type_code' property='messageTypeCode' />    <result column='content' property='content' />    <result column='push_interval' property='pushInterval' />    <result column='push_type_code' property='pushTypeCode' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertMessageTemplate' parameterType='BRP.data.MessageTemplate' keyProperty='id' useGeneratedKeys='true'>    insert into message_template    (id,company_id,pusher_code,push_time,message_type_code,content,push_interval,push_type_code,create_time,update_time)    values    (#{id},#{companyId},#{pusherCode},#{pushTime},#{messageTypeCode},#{content},#{pushInterval},#{pushTypeCode},now(),now())  </insert>  <select id='selectMessageTemplate' parameterType='java.lang.Integer' resultMap='MessageTemplateResultMap'>    select * from message_template where id=#{id}  </select>  <update id='updateMessageTemplate' parameterType='BRP.data.MessageTemplate'>    update message_template set id=#{id},company_id=#{companyId},pusher_code=#{pusherCode},push_time=#{pushTime},message_type_code=#{messageTypeCode},content=#{content},push_interval=#{pushInterval},push_type_code=#{pushTypeCode},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteMessageTemplate' parameterType='java.lang.Integer'>    delete from message_template where id=#{id}  </delete>  <insert id='insertMessageTemplates' parameterType='java.util.List'>    insert into message_template    (id,company_id,pusher_code,push_time,message_type_code,content,push_interval,push_type_code,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.pusherCode},#{item.pushTime},#{item.messageTypeCode},#{item.content},#{item.pushInterval},#{item.pushTypeCode},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectMessageTemplates' resultMap='MessageTemplateResultMap'>    select * from message_template  </select>  <select id='selectMessageTemplateBySql' parameterType='java.lang.String' resultMap='MessageTemplateResultMap'>    ${value}  </select>  <delete id='deleteMessageTemplateBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateMessageTemplateBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectMessageTemplateByCondition' parameterType='java.lang.String' resultMap='MessageTemplateResultMap'>    select * from message_template where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from message_template where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>