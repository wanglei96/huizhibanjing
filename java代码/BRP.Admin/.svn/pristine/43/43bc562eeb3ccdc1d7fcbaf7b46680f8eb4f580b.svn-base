<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewActivity'>  <resultMap type='BRP.data.ViewActivity' id='ViewActivityResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='name' property='name' />    <result column='activity_type_code' property='activityTypeCode' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />    <result column='renewal_price' property='renewalPrice' />    <result column='buy_days' property='buyDays' />    <result column='gift_days' property='giftDays' />    <result column='loanable_num' property='loanableNum' />    <result column='company_name' property='companyName' />    <result column='activity_type_name' property='activityTypeName' />  </resultMap>  <select id='selectViewActivitys' resultMap='ViewActivityResultMap'>    select * from view_activity  </select>  <select id='selectViewActivityById' parameterType='java.lang.Integer' resultMap='ViewActivityResultMap'>    select * from view_activity where id= #{id}  </select>  <select id='selectViewActivityBySql' parameterType='java.lang.String' resultMap='ViewActivityResultMap'>    ${value}  </select>  <select id='selectViewActivityByCondition' parameterType='java.lang.String' resultMap='ViewActivityResultMap'>    select * from view_activity where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_activity where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>