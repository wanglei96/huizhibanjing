<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysValidationDataType'>  <resultMap type='strosoft.sys.data.SysValidationDataType' id='SysValidationDataTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />  </resultMap>  <insert id='insertSysValidationDataType' parameterType='strosoft.sys.data.SysValidationDataType'>    insert into sys_validation_data_type    (code,name,description,display_order)    values    (#{code},#{name},#{description},#{displayOrder})  </insert>  <select id='selectSysValidationDataType' parameterType='java.lang.String' resultMap='SysValidationDataTypeResultMap'>    select * from sys_validation_data_type where code=#{code}  </select>  <update id='updateSysValidationDataType' parameterType='strosoft.sys.data.SysValidationDataType'>    update sys_validation_data_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysValidationDataType' parameterType='java.lang.String'>    delete from sys_validation_data_type where code=#{code}  </delete>  <insert id='insertSysValidationDataTypes' parameterType='java.util.List'>    insert into sys_validation_data_type    (code,name,description,display_order)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder})   </foreach>  </insert>  <select id='selectSysValidationDataTypes' resultMap='SysValidationDataTypeResultMap'>    select * from sys_validation_data_type  </select>  <select id='selectSysValidationDataTypeBySql' parameterType='java.lang.String' resultMap='SysValidationDataTypeResultMap'>    ${value}  </select>  <delete id='deleteSysValidationDataTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysValidationDataTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysValidationDataTypeByCondition' parameterType='java.lang.String' resultMap='SysValidationDataTypeResultMap'>    select * from sys_validation_data_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_validation_data_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>