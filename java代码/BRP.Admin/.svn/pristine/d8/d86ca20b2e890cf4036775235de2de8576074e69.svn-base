<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ActivityType'>  <resultMap type='BRP.data.ActivityType' id='ActivityTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertActivityType' parameterType='BRP.data.ActivityType'>    insert into activity_type    (code,name,description,display_order,enabled,create_time,update_time)    values    (#{code},#{name},#{description},#{displayOrder},#{enabled},now(),now())  </insert>  <select id='selectActivityType' parameterType='java.lang.String' resultMap='ActivityTypeResultMap'>    select * from activity_type where code=#{code}  </select>  <update id='updateActivityType' parameterType='BRP.data.ActivityType'>    update activity_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   <if test="enabled !=null">enabled=#{enabled},</if>   <if test="createTime !=null">create_time=#{createTime},</if>   <if test="updateTime !=null">update_time=#{updateTime},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteActivityType' parameterType='java.lang.String'>    delete from activity_type where code=#{code}  </delete>  <insert id='insertActivityTypes' parameterType='java.util.List'>    insert into activity_type    (code,name,description,display_order,enabled,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder},#{item.enabled},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectActivityTypes' resultMap='ActivityTypeResultMap'>    select * from activity_type  </select>  <select id='selectActivityTypeBySql' parameterType='java.lang.String' resultMap='ActivityTypeResultMap'>    ${value}  </select>  <delete id='deleteActivityTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateActivityTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectActivityTypeByCondition' parameterType='java.lang.String' resultMap='ActivityTypeResultMap'>    select * from activity_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from activity_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>