<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysOperationLog'>  <resultMap type='strosoft.sys.data.SysOperationLog' id='SysOperationLogResultMap' >    <result column='id' property='id' />    <result column='operation_target_code' property='operationTargetCode' />    <result column='operation_type_code' property='operationTypeCode' />    <result column='content' property='content' />    <result column='operator_id' property='operatorId' />    <result column='operate_time' property='operateTime' />    <result column='primary_key' property='primaryKey' />  </resultMap>  <insert id='insertSysOperationLog' parameterType='strosoft.sys.data.SysOperationLog' keyProperty='id' useGeneratedKeys='true'>    insert into sys_operation_log    (id,operation_target_code,operation_type_code,content,operator_id,operate_time,primary_key)    values    (#{id},#{operationTargetCode},#{operationTypeCode},#{content},#{operatorId},#{operateTime},#{primaryKey})  </insert>  <select id='selectSysOperationLog' parameterType='java.lang.Integer' resultMap='SysOperationLogResultMap'>    select * from sys_operation_log where id=#{id}  </select>  <update id='updateSysOperationLog' parameterType='strosoft.sys.data.SysOperationLog'>    update sys_operation_log set id=#{id},operation_target_code=#{operationTargetCode},operation_type_code=#{operationTypeCode},content=#{content},operator_id=#{operatorId},operate_time=#{operateTime},primary_key=#{primaryKey}    where id=#{id}  </update>  <delete id='deleteSysOperationLog' parameterType='java.lang.Integer'>    delete from sys_operation_log where id=#{id}  </delete>  <select id='selectSysOperationLogByOperationTargetCode' parameterType='java.lang.String' resultMap='SysOperationLogResultMap'>    select * from sys_operation_log where operation_target_code=#{operationTargetCode}  </select>  <select id='selectSysOperationLogByOperationTypeCode' parameterType='java.lang.String' resultMap='SysOperationLogResultMap'>    select * from sys_operation_log where operation_type_code=#{operationTypeCode}  </select>  <delete id='deleteSysOperationLogByOperationTargetCode' parameterType='java.lang.String'>    delete from sys_operation_log where operation_target_code=#{operationTargetCode}  </delete>  <delete id='deleteSysOperationLogByOperationTypeCode' parameterType='java.lang.String'>    delete from sys_operation_log where operation_type_code=#{operationTypeCode}  </delete>  <insert id='insertSysOperationLogs' parameterType='java.util.List'>    insert into sys_operation_log    (id,operation_target_code,operation_type_code,content,operator_id,operate_time,primary_key)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.operationTargetCode},#{item.operationTypeCode},#{item.content},#{item.operatorId},#{item.operateTime},#{item.primaryKey})   </foreach>  </insert>  <select id='selectSysOperationLogs' resultMap='SysOperationLogResultMap'>    select * from sys_operation_log  </select>  <select id='selectSysOperationLogBySql' parameterType='java.lang.String' resultMap='SysOperationLogResultMap'>    ${value}  </select>  <delete id='deleteSysOperationLogBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysOperationLogBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysOperationLogByCondition' parameterType='java.lang.String' resultMap='SysOperationLogResultMap'>    select * from sys_operation_log where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_operation_log where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>