<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewSysSetting'>  <resultMap type='strosoft.sys.data.ViewSysSetting' id='ViewSysSettingResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='value' property='value' />    <result column='sys_data_type_code' property='sysDataTypeCode' />    <result column='sys_setting_category_code' property='sysSettingCategoryCode' />    <result column='sys_ui_control_code' property='sysUiControlCode' />    <result column='required' property='required' />    <result column='readonly' property='readonly' />    <result column='remark' property='remark' />    <result column='display_order' property='displayOrder' />    <result column='format_string' property='formatString' />    <result column='hidden' property='hidden' />    <result column='sys_setting_category_name' property='sysSettingCategoryName' />    <result column='sys_ui_control_name' property='sysUiControlName' />    <result column='sys_data_type_name' property='sysDataTypeName' />    <result column='description' property='description' />    <result column='required_name' property='requiredName' />  </resultMap>  <select id='selectViewSysSettings' resultMap='ViewSysSettingResultMap'>    select * from view_sys_setting  </select>  <select id='selectViewSysSettingByCode' parameterType='java.lang.String' resultMap='ViewSysSettingResultMap'>    select * from view_sys_setting where code= #{code}  </select>  <select id='selectViewSysSettingBySql' parameterType='java.lang.String' resultMap='ViewSysSettingResultMap'>    ${value}  </select>  <select id='selectViewSysSettingByCondition' parameterType='java.lang.String' resultMap='ViewSysSettingResultMap'>    select * from view_sys_setting where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_sys_setting where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>