<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>登陆</title>
<link   rel="stylesheet" href="css/bootstrap.min.css"/>
<script type="text/javascript" src="../../js/jquery.js"></script>
<script type="text/javascript" src="../../js/common.js"></script>
<script type="text/javascript" src="../../js/config.js"></script>
<script type="text/javascript" src="../../js/json2.js"></script>
<script>
$(function() {
		$("#btnLog").click(function() {
			var login_name = $("#login_name").val();
			var password = $("#password").val();
			var data = new Object();
			data.tableName = "sys_user";
			data.login_name = login_name;
			data.password = password;
			call("Login", data, function(result) {
				//alert(JSON.stringify(result.data));
				window.location.href="Test.html";
			});
		});
	});
</script>
<style>
	#logDiv{
		width:500px;
		height:400px;
		border:1px #000000 solid;
		margin-left:30%;
		margin-top:10%;		
		}
	#logTab{
		width:300px;
		height:150px;
		margin-left:20%;
		margin-top:20%;
		}
</style>
</head>

<body>
	<div id="logDiv">
    	<table id="logTab" border="0" cellpadding="0" cellspacing="0">
        	<tr>
            	<td style="text-align:right">账号：</td>
                <td>
                	<input  type="text" id="login_name" name="login_name" value=""/>
                </td>
            </tr>
            <tr>
            	<td style="text-align:right">密码：</td>
                <td>
                	<input  type="password" id="password" name="password" value=""/>
                </td>
            </tr>
            <tr>
            	<td colspan="2" style="text-align:center">
                	<input type="button" id="btnLog" name="btnLog" value="登陆"/>
                    <input type="button" id="btnRe" name="btnRe" value="取消"/>
                </td>
            </tr>
        </table>
    </div>
</body>
</html>