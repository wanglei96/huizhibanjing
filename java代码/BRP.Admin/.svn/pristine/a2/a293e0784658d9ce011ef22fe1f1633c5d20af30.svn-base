<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanyChannel'>  <resultMap type='BRP.data.CompanyChannel' id='CompanyChannelResultMap' >    <result column='id' property='id' />    <result column='code' property='code' />    <result column='name' property='name' />    <result column='image_file_id' property='imageFileId' />    <result column='content' property='content' />    <result column='url' property='url' />    <result column='parent_id' property='parentId' />    <result column='display_order' property='displayOrder' />    <result column='company_id' property='companyId' />    <result column='company_user_id' property='companyUserId' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertCompanyChannel' parameterType='BRP.data.CompanyChannel' keyProperty='id' useGeneratedKeys='true'>    insert into company_channel    (id,code,name,image_file_id,content,url,parent_id,display_order,company_id,company_user_id,create_time,update_time)    values    (#{id},#{code},#{name},#{imageFileId},#{content},#{url},#{parentId},#{displayOrder},#{companyId},#{companyUserId},now(),now())  </insert>  <select id='selectCompanyChannel' parameterType='java.lang.Integer' resultMap='CompanyChannelResultMap'>    select * from company_channel where id=#{id}  </select>  <update id='updateCompanyChannel' parameterType='BRP.data.CompanyChannel'>    update company_channel set id=#{id},code=#{code},name=#{name},image_file_id=#{imageFileId},content=#{content},url=#{url},parent_id=#{parentId},display_order=#{displayOrder},company_id=#{companyId},company_user_id=#{companyUserId},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteCompanyChannel' parameterType='java.lang.Integer'>    delete from company_channel where id=#{id}  </delete>  <select id='selectCompanyChannelByCompanyId' parameterType='java.lang.Integer' resultMap='CompanyChannelResultMap'>    select * from company_channel where company_id=#{companyId}  </select>  <delete id='deleteCompanyChannelByCompanyId' parameterType='java.lang.Integer'>    delete from company_channel where company_id=#{companyId}  </delete>  <insert id='insertCompanyChannels' parameterType='java.util.List'>    insert into company_channel    (id,code,name,image_file_id,content,url,parent_id,display_order,company_id,company_user_id,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.code},#{item.name},#{item.imageFileId},#{item.content},#{item.url},#{item.parentId},#{item.displayOrder},#{item.companyId},#{item.companyUserId},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectCompanyChannels' resultMap='CompanyChannelResultMap'>    select * from company_channel  </select>  <select id='selectCompanyChannelBySql' parameterType='java.lang.String' resultMap='CompanyChannelResultMap'>    ${value}  </select>  <delete id='deleteCompanyChannelBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanyChannelBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanyChannelByCondition' parameterType='java.lang.String' resultMap='CompanyChannelResultMap'>    select * from company_channel where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_channel where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>