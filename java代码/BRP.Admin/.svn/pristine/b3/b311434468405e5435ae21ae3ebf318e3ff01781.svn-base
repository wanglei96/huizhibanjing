<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CompanyImage'>  <resultMap type='BRP.data.CompanyImage' id='CompanyImageResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='image_file_id' property='imageFileId' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertCompanyImage' parameterType='BRP.data.CompanyImage' keyProperty='id' useGeneratedKeys='true'>    insert into company_image    (id,company_id,image_file_id,create_time,update_time)    values    (#{id},#{companyId},#{imageFileId},now(),now())  </insert>  <select id='selectCompanyImage' parameterType='java.lang.Integer' resultMap='CompanyImageResultMap'>    select * from company_image where id=#{id}  </select>  <update id='updateCompanyImage' parameterType='BRP.data.CompanyImage'>    update company_image set id=#{id},company_id=#{companyId},image_file_id=#{imageFileId},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteCompanyImage' parameterType='java.lang.Integer'>    delete from company_image where id=#{id}  </delete>  <select id='selectCompanyImageByCompanyId' parameterType='java.lang.Integer' resultMap='CompanyImageResultMap'>    select * from company_image where company_id=#{companyId}  </select>  <select id='selectCompanyImageByImageFileId' parameterType='java.lang.Integer' resultMap='CompanyImageResultMap'>    select * from company_image where image_file_id=#{imageFileId}  </select>  <delete id='deleteCompanyImageByCompanyId' parameterType='java.lang.Integer'>    delete from company_image where company_id=#{companyId}  </delete>  <delete id='deleteCompanyImageByImageFileId' parameterType='java.lang.Integer'>    delete from company_image where image_file_id=#{imageFileId}  </delete>  <insert id='insertCompanyImages' parameterType='java.util.List'>    insert into company_image    (id,company_id,image_file_id,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.imageFileId},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectCompanyImages' resultMap='CompanyImageResultMap'>    select * from company_image  </select>  <select id='selectCompanyImageBySql' parameterType='java.lang.String' resultMap='CompanyImageResultMap'>    ${value}  </select>  <delete id='deleteCompanyImageBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCompanyImageBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCompanyImageByCondition' parameterType='java.lang.String' resultMap='CompanyImageResultMap'>    select * from company_image where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from company_image where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>