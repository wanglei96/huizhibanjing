<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='MemberBorrowTotal'>  <resultMap type='BRP.data.MemberBorrowTotal' id='MemberBorrowTotalResultMap' >  </resultMap>  <select id='selectMemberBorrowTotals' resultMap='MemberBorrowTotalResultMap'>    select * from member_borrow_total  </select>  <select id='selectMemberBorrowTotalBySql' parameterType='java.lang.String' resultMap='MemberBorrowTotalResultMap'>    ${value}  </select>  <select id='selectMemberBorrowTotalByCondition' parameterType='java.lang.String' resultMap='MemberBorrowTotalResultMap'>    select * from member_borrow_total where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from member_borrow_total where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>