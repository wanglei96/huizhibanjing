package strosoft.cms.data;

public class CmsGeneralEntity {
	private String PoweredBy;//	PoweredBy 链接
	private String SiteName;//	站点名称
	private String SiteId;//	站点Id
	private String SiteDir;//	站点文件夹，如果为总站点，值为空
	private String SiteUrl;//	站点根目录地址
	private String RootUrl;//	系统根目录地址
	private String CurrentUrl;//	当前页地址
	private String ChannelUrl;//	栏目页地址
	public String getPoweredBy() {
		return PoweredBy;
	}
	public void setPoweredBy(String poweredBy) {
		PoweredBy = poweredBy;
	}
	public String getSiteName() {
		return SiteName;
	}
	public void setSiteName(String siteName) {
		SiteName = siteName;
	}
	public String getSiteId() {
		return SiteId;
	}
	public void setSiteId(String siteId) {
		SiteId = siteId;
	}
	public String getSiteDir() {
		return SiteDir;
	}
	public void setSiteDir(String siteDir) {
		SiteDir = siteDir;
	}
	public String getSiteUrl() {
		return SiteUrl;
	}
	public void setSiteUrl(String siteUrl) {
		SiteUrl = siteUrl;
	}
	public String getRootUrl() {
		return RootUrl;
	}
	public void setRootUrl(String rootUrl) {
		RootUrl = rootUrl;
	}
	public String getCurrentUrl() {
		return CurrentUrl;
	}
	public void setCurrentUrl(String currentUrl) {
		CurrentUrl = currentUrl;
	}
	public String getChannelUrl() {
		return ChannelUrl;
	}
	public void setChannelUrl(String channelUrl) {
		ChannelUrl = channelUrl;
	}

}
