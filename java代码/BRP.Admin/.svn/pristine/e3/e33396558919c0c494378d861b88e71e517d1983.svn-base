<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='CmsContentTag'>  <resultMap type='strosoft.cms.data.CmsContentTag' id='CmsContentTagResultMap' >    <result column='cms_content_id' property='cmsContentId' />    <result column='tag' property='tag' />  </resultMap>  <insert id='insertCmsContentTag' parameterType='strosoft.cms.data.CmsContentTag'>    insert into cms_content_tag    (cms_content_id,tag)    values    (#{cmsContentId},#{tag})  </insert>  <select id='selectCmsContentTag' parameterType='strosoft.cms.data.CmsContentTag' resultMap='CmsContentTagResultMap'>    select * from cms_content_tag where cms_content_id=#{cmsContentId} and tag=#{tag}  </select>  <update id='updateCmsContentTag' parameterType='strosoft.cms.data.CmsContentTag'>    update cms_content_tag  <trim prefix="set" suffixOverrides=",">    <if test="cmsContentId !=null">cms_content_id=#{cmsContentId},</if>   <if test="tag !=null">tag=#{tag},</if>   </trim>   <where>    cms_content_id=#{cmsContentId} and tag=#{tag}  </where>  </update>  <delete id='deleteCmsContentTag' parameterType='strosoft.cms.data.CmsContentTag'>    delete from cms_content_tag where cms_content_id=#{cmsContentId} and tag=#{tag}  </delete>  <insert id='insertCmsContentTags' parameterType='java.util.List'>    insert into cms_content_tag    (cms_content_id,tag)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.cmsContentId},#{item.tag})   </foreach>  </insert>  <select id='selectCmsContentTags' resultMap='CmsContentTagResultMap'>    select * from cms_content_tag  </select>  <select id='selectCmsContentTagBySql' parameterType='java.lang.String' resultMap='CmsContentTagResultMap'>    ${value}  </select>  <delete id='deleteCmsContentTagBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateCmsContentTagBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectCmsContentTagByCondition' parameterType='java.lang.String' resultMap='CmsContentTagResultMap'>    select * from cms_content_tag where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from cms_content_tag where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>