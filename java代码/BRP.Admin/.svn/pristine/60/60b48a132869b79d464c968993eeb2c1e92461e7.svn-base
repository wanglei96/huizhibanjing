<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysOrganization'>  <resultMap type='strosoft.sys.data.SysOrganization' id='SysOrganizationResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='parent_id' property='parentId' />    <result column='display_order' property='displayOrder' />    <result column='description' property='description' />    <result column='create_time' property='createTime' />  </resultMap>  <insert id='insertSysOrganization' parameterType='strosoft.sys.data.SysOrganization' keyProperty='id' useGeneratedKeys='true'>    insert into sys_organization    (id,name,parent_id,display_order,description,create_time)    values    (#{id},#{name},#{parentId},#{displayOrder},#{description},now())  </insert>  <select id='selectSysOrganization' parameterType='java.lang.Integer' resultMap='SysOrganizationResultMap'>    select * from sys_organization where id=#{id}  </select>  <update id='updateSysOrganization' parameterType='strosoft.sys.data.SysOrganization'>    update sys_organization set id=#{id},name=#{name},parent_id=#{parentId},display_order=#{displayOrder},description=#{description},create_time=#{createTime}    where id=#{id}  </update>  <delete id='deleteSysOrganization' parameterType='java.lang.Integer'>    delete from sys_organization where id=#{id}  </delete>  <insert id='insertSysOrganizations' parameterType='java.util.List'>    insert into sys_organization    (id,name,parent_id,display_order,description,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.parentId},#{item.displayOrder},#{item.description},now())   </foreach>  </insert>  <select id='selectSysOrganizations' resultMap='SysOrganizationResultMap'>    select * from sys_organization  </select>  <select id='selectSysOrganizationBySql' parameterType='java.lang.String' resultMap='SysOrganizationResultMap'>    ${value}  </select>  <delete id='deleteSysOrganizationBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysOrganizationBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysOrganizationByCondition' parameterType='java.lang.String' resultMap='SysOrganizationResultMap'>    select * from sys_organization where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_organization where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>