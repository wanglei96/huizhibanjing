<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ActivityRecommend'>  <resultMap type='BRP.data.ActivityRecommend' id='ActivityRecommendResultMap' >    <result column='id' property='id' />    <result column='company_id' property='companyId' />    <result column='referrer_card_id' property='referrerCardId' />    <result column='new_member_card_id' property='newMemberCardId' />    <result column='days' property='days' />    <result column='activity_id' property='activityId' />    <result column='open_card_time' property='openCardTime' />    <result column='create_time' property='createTime' />  </resultMap>  <insert id='insertActivityRecommend' parameterType='BRP.data.ActivityRecommend' keyProperty='id' useGeneratedKeys='true'>    insert into activity_recommend    (id,company_id,referrer_card_id,new_member_card_id,days,activity_id,open_card_time,create_time)    values    (#{id},#{companyId},#{referrerCardId},#{newMemberCardId},#{days},#{activityId},#{openCardTime},now())  </insert>  <select id='selectActivityRecommend' parameterType='java.lang.Integer' resultMap='ActivityRecommendResultMap'>    select * from activity_recommend where id=#{id}  </select>  <update id='updateActivityRecommend' parameterType='BRP.data.ActivityRecommend'>    update activity_recommend set id=#{id},company_id=#{companyId},referrer_card_id=#{referrerCardId},new_member_card_id=#{newMemberCardId},days=#{days},activity_id=#{activityId},open_card_time=#{openCardTime},create_time=#{createTime}    where id=#{id}  </update>  <delete id='deleteActivityRecommend' parameterType='java.lang.Integer'>    delete from activity_recommend where id=#{id}  </delete>  <select id='selectActivityRecommendByActivityId' parameterType='java.lang.Integer' resultMap='ActivityRecommendResultMap'>    select * from activity_recommend where activity_id=#{activityId}  </select>  <select id='selectActivityRecommendByCompanyId' parameterType='java.lang.Integer' resultMap='ActivityRecommendResultMap'>    select * from activity_recommend where company_id=#{companyId}  </select>  <select id='selectActivityRecommendByNewMemberCardId' parameterType='java.lang.Integer' resultMap='ActivityRecommendResultMap'>    select * from activity_recommend where new_member_card_id=#{newMemberCardId}  </select>  <select id='selectActivityRecommendByReferrerCardId' parameterType='java.lang.Integer' resultMap='ActivityRecommendResultMap'>    select * from activity_recommend where referrer_card_id=#{referrerCardId}  </select>  <delete id='deleteActivityRecommendByActivityId' parameterType='java.lang.Integer'>    delete from activity_recommend where activity_id=#{activityId}  </delete>  <delete id='deleteActivityRecommendByCompanyId' parameterType='java.lang.Integer'>    delete from activity_recommend where company_id=#{companyId}  </delete>  <delete id='deleteActivityRecommendByNewMemberCardId' parameterType='java.lang.Integer'>    delete from activity_recommend where new_member_card_id=#{newMemberCardId}  </delete>  <delete id='deleteActivityRecommendByReferrerCardId' parameterType='java.lang.Integer'>    delete from activity_recommend where referrer_card_id=#{referrerCardId}  </delete>  <insert id='insertActivityRecommends' parameterType='java.util.List'>    insert into activity_recommend    (id,company_id,referrer_card_id,new_member_card_id,days,activity_id,open_card_time,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.companyId},#{item.referrerCardId},#{item.newMemberCardId},#{item.days},#{item.activityId},#{item.openCardTime},now())   </foreach>  </insert>  <select id='selectActivityRecommends' resultMap='ActivityRecommendResultMap'>    select * from activity_recommend  </select>  <select id='selectActivityRecommendBySql' parameterType='java.lang.String' resultMap='ActivityRecommendResultMap'>    ${value}  </select>  <delete id='deleteActivityRecommendBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateActivityRecommendBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectActivityRecommendByCondition' parameterType='java.lang.String' resultMap='ActivityRecommendResultMap'>    select * from activity_recommend where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from activity_recommend where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>