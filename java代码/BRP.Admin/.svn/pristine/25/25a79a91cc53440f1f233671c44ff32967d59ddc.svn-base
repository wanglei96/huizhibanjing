<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ViewCmsComment'>  <resultMap type='strosoft.cms.data.ViewCmsComment' id='ViewCmsCommentResultMap' >    <result column='id' property='id' />    <result column='content' property='content' />    <result column='create_time' property='createTime' />    <result column='cms_comment_status_code' property='cmsCommentStatusCode' />    <result column='cms_channel_id' property='cmsChannelId' />    <result column='cms_content_id' property='cmsContentId' />    <result column='cms_site_id' property='cmsSiteId' />    <result column='display_order' property='displayOrder' />    <result column='name' property='name' />    <result column='title' property='title' />  </resultMap>  <select id='selectViewCmsComments' resultMap='ViewCmsCommentResultMap'>    select * from view_cms_comment  </select>  <select id='selectViewCmsCommentById' parameterType='java.lang.Integer' resultMap='ViewCmsCommentResultMap'>    select * from view_cms_comment where id= #{id}  </select>  <select id='selectViewCmsCommentBySql' parameterType='java.lang.String' resultMap='ViewCmsCommentResultMap'>    ${value}  </select>  <select id='selectViewCmsCommentByCondition' parameterType='java.lang.String' resultMap='ViewCmsCommentResultMap'>    select * from view_cms_comment where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from view_cms_comment where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select></mapper>