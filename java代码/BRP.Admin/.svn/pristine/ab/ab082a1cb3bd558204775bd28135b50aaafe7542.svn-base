<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='ActivityRecommendError'>  <resultMap type='BRP.data.ActivityRecommendError' id='ActivityRecommendErrorResultMap' >    <result column='id' property='id' />    <result column='member_card_no' property='memberCardNo' />    <result column='referrer_no' property='referrerNo' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertActivityRecommendError' parameterType='BRP.data.ActivityRecommendError' keyProperty='id' useGeneratedKeys='true'>    insert into activity_recommend_error    (id,member_card_no,referrer_no,create_time,update_time)    values    (#{id},#{memberCardNo},#{referrerNo},now(),now())  </insert>  <select id='selectActivityRecommendError' parameterType='java.lang.Integer' resultMap='ActivityRecommendErrorResultMap'>    select * from activity_recommend_error where id=#{id}  </select>  <update id='updateActivityRecommendError' parameterType='BRP.data.ActivityRecommendError'>    update activity_recommend_error set id=#{id},member_card_no=#{memberCardNo},referrer_no=#{referrerNo},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteActivityRecommendError' parameterType='java.lang.Integer'>    delete from activity_recommend_error where id=#{id}  </delete>  <insert id='insertActivityRecommendErrors' parameterType='java.util.List'>    insert into activity_recommend_error    (id,member_card_no,referrer_no,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.memberCardNo},#{item.referrerNo},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectActivityRecommendErrors' resultMap='ActivityRecommendErrorResultMap'>    select * from activity_recommend_error  </select>  <select id='selectActivityRecommendErrorBySql' parameterType='java.lang.String' resultMap='ActivityRecommendErrorResultMap'>    ${value}  </select>  <delete id='deleteActivityRecommendErrorBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateActivityRecommendErrorBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectActivityRecommendErrorByCondition' parameterType='java.lang.String' resultMap='ActivityRecommendErrorResultMap'>    select * from activity_recommend_error where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from activity_recommend_error where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>