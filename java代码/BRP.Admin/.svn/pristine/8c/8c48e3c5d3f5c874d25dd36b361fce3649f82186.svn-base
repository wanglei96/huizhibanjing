package strosoft.cms.data;

import java.util.HashMap;

public class CmsContentsContext {
	//private String align; // "整体对齐"
	//private String cellPadding; // "填充"
	//private String cellSpacing; // "间距"
	private String channelIndex; // "栏目索引"
	private String channelCode; // "栏目索引"
	private String channelName; // "栏目名称"
	// private String class; //"Css类"
	//private String columns; // "列数"
	//private String direction; // "方向"
	//private String group; // "指定显示的内容组"
	private String groupChannel; // "指定显示的栏目组"
	private String groupChannelNot; // "指定不显示的栏目组"
	//private String groupContent; // "指定显示的内容组"
	//private String groupContentNot; // "指定不显示的内容组"
	//private String groupNot; // "指定不显示的内容组"
	//private String height; // "整体高度"
	private String isColor; // "仅显示醒目内容"
	//private String isDynamic; // "是否动态显示"
	private String isFile; // "仅显示附件内容"
	private String isHot; // "仅显示热点内容"
	private String isImage; // "仅显示图片内容"
	private String isNoDup; // "不显示重复标题的内容"
	private String isRecommend; // "仅显示推荐内容"
	//private String isRelatedContents; // "显示相关内容列表"
	private String isTop; // "仅显示置顶内容"
	private String isVideo; // "仅显示视频内容"
	//private String itemAlign; // "项水平对齐"
	//private String itemClass; // "项Css类"
	//private String itemHeight; // "项高度"
	//private String itemVerticalAlign; // "项垂直对齐"
	//private String itemWidth; // "项宽度"
	//private String layout; // "指定列表布局方式"
	private String order; // "排序"
	private String scope; // "内容范围"
	private String startNum; // "从第几条信息开始显示"
	private String pageNum;//每一页显示的栏目数目
	//private String tags; // "指定标签"
	private String topLevel; // "从首页向下的栏目级别"
	private String totalNum; // "显示内容数目"
	private String upLevel; // "上级栏目的级别"
	private String where; // "获取内容列表的条件判断"
	//private String width; // "整体宽度"
	public CmsContentsContext(){
		
	}
	public CmsContentsContext(HashMap<String, String> attributeCondition) {
		this.channelIndex = attributeCondition.get("channelIndex".toLowerCase());
		this.channelCode = attributeCondition.get("channelCode".toLowerCase());
		this.channelName = attributeCondition.get("channelName".toLowerCase());
		this.groupChannel = attributeCondition.get("groupChannel".toLowerCase());
		this.groupChannelNot = attributeCondition.get("groupChannelNot".toLowerCase());
		this.isColor = attributeCondition.get("isColor".toLowerCase());
		this.isFile = attributeCondition.get("isFile".toLowerCase());
		this.isHot = attributeCondition.get("isHot".toLowerCase());
		this.isImage = attributeCondition.get("isImage".toLowerCase());
		this.isNoDup = attributeCondition.get("isNoDup".toLowerCase());
		this.isRecommend = attributeCondition.get("isRecommend".toLowerCase());
		this.isTop = attributeCondition.get("isTop".toLowerCase());
		this.isVideo = attributeCondition.get("isVideo".toLowerCase());
		this.order = attributeCondition.get("order".toLowerCase());
		this.scope = attributeCondition.get("scope".toLowerCase());
		this.startNum = attributeCondition.get("startNum".toLowerCase());
		this.pageNum = attributeCondition.get("pageNum".toLowerCase());
		this.topLevel = attributeCondition.get("topLevel".toLowerCase());
		this.totalNum = attributeCondition.get("totalNum".toLowerCase());
		this.upLevel = attributeCondition.get("upLevel".toLowerCase());
		this.where = attributeCondition.get("where".toLowerCase());
	}
	public String getChannelIndex() {
		return channelIndex;
	}
	public void setChannelIndex(String channelIndex) {
		this.channelIndex = channelIndex;
	}
	public String getChannelCode() {
		return channelCode;
	}
	public void setChannelCode(String channelCode) {
		this.channelCode = channelCode;
	}
	public String getChannelName() {
		return channelName;
	}
	public void setChannelName(String channelName) {
		this.channelName = channelName;
	}
	public String getGroupChannel() {
		return groupChannel;
	}
	public void setGroupChannel(String groupChannel) {
		this.groupChannel = groupChannel;
	}
	public String getGroupChannelNot() {
		return groupChannelNot;
	}
	public void setGroupChannelNot(String groupChannelNot) {
		this.groupChannelNot = groupChannelNot;
	}
	public String getIsColor() {
		return isColor;
	}
	public void setIsColor(String isColor) {
		this.isColor = isColor;
	}
	public String getIsFile() {
		return isFile;
	}
	public void setIsFile(String isFile) {
		this.isFile = isFile;
	}
	public String getIsHot() {
		return isHot;
	}
	public void setIsHot(String isHot) {
		this.isHot = isHot;
	}
	public String getIsImage() {
		return isImage;
	}
	public void setIsImage(String isImage) {
		this.isImage = isImage;
	}
	public String getIsNoDup() {
		return isNoDup;
	}
	public void setIsNoDup(String isNoDup) {
		this.isNoDup = isNoDup;
	}
	public String getIsRecommend() {
		return isRecommend;
	}
	public void setIsRecommend(String isRecommend) {
		this.isRecommend = isRecommend;
	}
	public String getIsTop() {
		return isTop;
	}
	public void setIsTop(String isTop) {
		this.isTop = isTop;
	}
	public String getIsVideo() {
		return isVideo;
	}
	public void setIsVideo(String isVideo) {
		this.isVideo = isVideo;
	}
	public String getOrder() {
		return order;
	}
	public void setOrder(String order) {
		this.order = order;
	}
	public String getScope() {
		return scope;
	}
	public void setScope(String scope) {
		this.scope = scope;
	}
	public String getStartNum() {
		return startNum;
	}
	public void setStartNum(String startNum) {
		this.startNum = startNum;
	}
	public String getTopLevel() {
		return topLevel;
	}
	public void setTopLevel(String topLevel) {
		this.topLevel = topLevel;
	}
	public String getTotalNum() {
		return totalNum;
	}
	public void setTotalNum(String totalNum) {
		this.totalNum = totalNum;
	}
	public String getUpLevel() {
		return upLevel;
	}
	public void setUpLevel(String upLevel) {
		this.upLevel = upLevel;
	}
	public String getWhere() {
		return where;
	}
	public void setWhere(String where) {
		this.where = where;
	}
	public String getPageNum() {
		return pageNum;
	}
	public void setPageNum(String pageNum) {
		this.pageNum = pageNum;
	}
	
}
