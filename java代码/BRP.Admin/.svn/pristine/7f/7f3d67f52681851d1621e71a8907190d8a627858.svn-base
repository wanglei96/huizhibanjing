package strosoft.cg.data;

import java.util.List;

import strosoft.app.data.DataEntity;
import strosoft.cg.common.SearchItemInfo;

public class CgView  extends DataEntity{
	private String viewName;
	private String type;
	private String moduleName;
	private String moduleDocument;
	private String modulePath;//moduleDocument.replace(".", "/")
	private String formattedViewName;
	private String dataSource;
	private List<CgViewColumn> viewColumns;
	private List<SearchItemInfo> searchItems;
	public String getViewName() {
		return viewName;
	}
	public void setViewName(String viewName) {
		this.viewName = viewName;
	}
	public List<SearchItemInfo> getSearchItems() {
		return searchItems;
	}
	public void setSearchItems(List<SearchItemInfo> searchItems) {
		this.searchItems = searchItems;
	}
	public String getModuleName() {
		return moduleName;
	}
	public void setModuleName(String moduleName) {
		this.moduleName = moduleName;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getModuleDocument() {
		return moduleDocument;
	}
	public void setModuleDocument(String moduleDocument) {
		this.moduleDocument = moduleDocument;
	}
	public List<CgViewColumn> getViewColumns() {
		return viewColumns;
	}
	public void setViewColumns(List<CgViewColumn> viewColumns) {
		this.viewColumns = viewColumns;
	}
	public String getDataSource() {
		return dataSource;
	}
	public void setDataSource(String dataSource) {
		this.dataSource = dataSource;
	}
	public String getFormattedViewName() {
		return formattedViewName;
	}
	public void setFormattedViewName(String formattedViewName) {
		this.formattedViewName = formattedViewName;
	}
	public String getModulePath() {
		return modulePath;
	}
	public void setModulePath(String modulePath) {
		this.modulePath = modulePath;
	}
	
	
}
