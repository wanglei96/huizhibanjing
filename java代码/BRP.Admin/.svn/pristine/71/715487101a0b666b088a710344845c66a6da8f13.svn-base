meterType='java.lang.Integer'>    delete from read_activity_member where read_activity_id=#{readActivityId}  </delete>  <insert id='insertReadActivityMembers' parameterType='java.util.List'>    insert into read_activity_member    (id,read_activity_id,member_id,create_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.readActivityId},#{item.memberId},now())   </foreach>  </insert>  <select id='selectReadActivityMembers' resultMap='ReadActivityMemberResultMap'>    select * from read_activity_member  </select>  <select id='selectReadActivityMemberBySql' parameterType='java.lang.String' resultMap='ReadActivityMemberResultMap'>    ${value}  </select>  <delete id='deleteReadActivityMemberBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateReadActivityMemberBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectReadActivityMemberByCondition' parameterType='java.lang.String' resultMap='ReadActivityMemberResultMap'>    select * from read_activity_member where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from read_activity_member where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>