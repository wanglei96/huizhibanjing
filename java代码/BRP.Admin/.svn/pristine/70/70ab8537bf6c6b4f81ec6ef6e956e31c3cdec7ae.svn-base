<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='PayOrderType'>  <resultMap type='BRP.data.PayOrderType' id='PayOrderTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />    <result column='is_deleted' property='isDeleted' />  </resultMap>  <insert id='insertPayOrderType' parameterType='BRP.data.PayOrderType'>    insert into pay_order_type    (code,name,description,display_order,enabled,create_time,update_time,is_deleted)    values    (#{code},#{name},#{description},#{displayOrder},#{enabled},now(),now(),#{isDeleted})  </insert>  <select id='selectPayOrderType' parameterType='java.lang.String' resultMap='PayOrderTypeResultMap'>    select * from pay_order_type where code=#{code}  </select>  <update id='updatePayOrderType' parameterType='BRP.data.PayOrderType'>    update pay_order_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="description !=null">description=#{description},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   <if test="enabled !=null">enabled=#{enabled},</if>   <if test="createTime !=null">create_time=#{createTime},</if>   <if test="updateTime !=null">update_time=#{updateTime},</if>   <if test="isDeleted !=null">is_deleted=#{isDeleted},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deletePayOrderType' parameterType='java.lang.String'>    delete from pay_order_type where code=#{code}  </delete>  <insert id='insertPayOrderTypes' parameterType='java.util.List'>    insert into pay_order_type    (code,name,description,display_order,enabled,create_time,update_time,is_deleted)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.description},#{item.displayOrder},#{item.enabled},now(),#{item.updateTime},#{item.isDeleted})   </foreach>  </insert>  <select id='selectPayOrderTypes' resultMap='PayOrderTypeResultMap'>    select * from pay_order_type  </select>  <select id='selectPayOrderTypeBySql' parameterType='java.lang.String' resultMap='PayOrderTypeResultMap'>    ${value}  </select>  <delete id='deletePayOrderTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updatePayOrderTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectPayOrderTypeByCondition' parameterType='java.lang.String' resultMap='PayOrderTypeResultMap'>    select * from pay_order_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from pay_order_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>