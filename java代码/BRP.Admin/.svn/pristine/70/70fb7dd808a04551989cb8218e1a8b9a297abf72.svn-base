<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='Classes'>  <resultMap type='BRP.data.Classes' id='ClassesResultMap' >    <result column='id' property='id' />    <result column='name' property='name' />    <result column='description' property='description' />    <result column='display_order' property='displayOrder' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertClasses' parameterType='BRP.data.Classes' keyProperty='id' useGeneratedKeys='true'>    insert into classes    (id,name,description,display_order,enabled,create_time,update_time)    values    (#{id},#{name},#{description},#{displayOrder},#{enabled},now(),now())  </insert>  <select id='selectClasses' parameterType='java.lang.Integer' resultMap='ClassesResultMap'>    select * from classes where id=#{id}  </select>  <update id='updateClasses' parameterType='BRP.data.Classes'>    update classes set id=#{id},name=#{name},description=#{description},display_order=#{displayOrder},enabled=#{enabled},create_time=#{createTime},update_time=#{updateTime}    where id=#{id}  </update>  <delete id='deleteClasses' parameterType='java.lang.Integer'>    delete from classes where id=#{id}  </delete>  <insert id='insertClassess' parameterType='java.util.List'>    insert into classes    (id,name,description,display_order,enabled,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.name},#{item.description},#{item.displayOrder},#{item.enabled},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectClassess' resultMap='ClassesResultMap'>    select * from classes  </select>  <select id='selectClassesBySql' parameterType='java.lang.String' resultMap='ClassesResultMap'>    ${value}  </select>  <delete id='deleteClassesBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateClassesBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectClassesByCondition' parameterType='java.lang.String' resultMap='ClassesResultMap'>    select * from classes where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from classes where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>