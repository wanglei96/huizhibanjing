<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='MemberScore'>  <resultMap type='BRP.data.MemberScore' id='MemberScoreResultMap' >    <result column='id' property='id' />    <result column='member_id' property='memberId' />    <result column='member_score_type_code' property='memberScoreTypeCode' />    <result column='score' property='score' />    <result column='score_time' property='scoreTime' />    <result column='create_time' property='createTime' />    <result column='operator' property='operator' />    <result column='type' property='type' />  </resultMap>  <insert id='insertMemberScore' parameterType='BRP.data.MemberScore' keyProperty='id' useGeneratedKeys='true'>    insert into member_score    (id,member_id,member_score_type_code,score,score_time,create_time,operator,type)    values    (#{id},#{memberId},#{memberScoreTypeCode},#{score},#{scoreTime},now(),#{operator},#{type})  </insert>  <select id='selectMemberScore' parameterType='java.lang.Integer' resultMap='MemberScoreResultMap'>    select * from member_score where id=#{id}  </select>  <update id='updateMemberScore' parameterType='BRP.data.MemberScore'>    update member_score set id=#{id},member_id=#{memberId},member_score_type_code=#{memberScoreTypeCode},score=#{score},score_time=#{scoreTime},create_time=#{createTime},operator=#{operator},type=#{type}    where id=#{id}  </update>  <delete id='deleteMemberScore' parameterType='java.lang.Integer'>    delete from member_score where id=#{id}  </delete>  <select id='selectMemberScoreByMemberId' parameterType='java.lang.Integer' resultMap='MemberScoreResultMap'>    select * from member_score where member_id=#{memberId}  </select>  <select id='selectMemberScoreByMemberScoreTypeCode' parameterType='java.lang.String' resultMap='MemberScoreResultMap'>    select * from member_score where member_score_type_code=#{memberScoreTypeCode}  </select>  <delete id='deleteMemberScoreByMemberId' parameterType='java.lang.Integer'>    delete from member_score where member_id=#{memberId}  </delete>  <delete id='deleteMemberScoreByMemberScoreTypeCode' parameterType='java.lang.String'>    delete from member_score where member_score_type_code=#{memberScoreTypeCode}  </delete>  <insert id='insertMemberScores' parameterType='java.util.List'>    insert into member_score    (id,member_id,member_score_type_code,score,score_time,create_time,operator,type)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.id},#{item.memberId},#{item.memberScoreTypeCode},#{item.score},#{item.scoreTime},now(),#{item.operator},#{item.type})   </foreach>  </insert>  <select id='selectMemberScores' resultMap='MemberScoreResultMap'>    select * from member_score  </select>  <select id='selectMemberScoreBySql' parameterType='java.lang.String' resultMap='MemberScoreResultMap'>    ${value}  </select>  <delete id='deleteMemberScoreBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateMemberScoreBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectMemberScoreByCondition' parameterType='java.lang.String' resultMap='MemberScoreResultMap'>    select * from member_score where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from member_score where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>