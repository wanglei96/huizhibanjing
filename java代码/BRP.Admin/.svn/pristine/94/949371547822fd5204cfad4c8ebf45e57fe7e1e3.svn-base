<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'><mapper namespace='SysMenuType'>  <resultMap type='strosoft.sys.data.SysMenuType' id='SysMenuTypeResultMap' >    <result column='code' property='code' />    <result column='name' property='name' />    <result column='display_order' property='displayOrder' />    <result column='description' property='description' />    <result column='enabled' property='enabled' />    <result column='create_time' property='createTime' />    <result column='update_time' property='updateTime' />  </resultMap>  <insert id='insertSysMenuType' parameterType='strosoft.sys.data.SysMenuType'>    insert into sys_menu_type    (code,name,display_order,description,enabled,create_time,update_time)    values    (#{code},#{name},#{displayOrder},#{description},#{enabled},now(),now())  </insert>  <select id='selectSysMenuType' parameterType='java.lang.String' resultMap='SysMenuTypeResultMap'>    select * from sys_menu_type where code=#{code}  </select>  <update id='updateSysMenuType' parameterType='strosoft.sys.data.SysMenuType'>    update sys_menu_type  <trim prefix="set" suffixOverrides=",">    <if test="code !=null">code=#{code},</if>   <if test="name !=null">name=#{name},</if>   <if test="displayOrder !=null">display_order=#{displayOrder},</if>   <if test="description !=null">description=#{description},</if>   <if test="enabled !=null">enabled=#{enabled},</if>   <if test="createTime !=null">create_time=#{createTime},</if>   <if test="updateTime !=null">update_time=#{updateTime},</if>   </trim>   <where>    code=#{code}  </where>  </update>  <delete id='deleteSysMenuType' parameterType='java.lang.String'>    delete from sys_menu_type where code=#{code}  </delete>  <insert id='insertSysMenuTypes' parameterType='java.util.List'>    insert into sys_menu_type    (code,name,display_order,description,enabled,create_time,update_time)    values   <foreach collection='list' item='item' index='index' separator=','>    (#{item.code},#{item.name},#{item.displayOrder},#{item.description},#{item.enabled},now(),#{item.updateTime})   </foreach>  </insert>  <select id='selectSysMenuTypes' resultMap='SysMenuTypeResultMap'>    select * from sys_menu_type  </select>  <select id='selectSysMenuTypeBySql' parameterType='java.lang.String' resultMap='SysMenuTypeResultMap'>    ${value}  </select>  <delete id='deleteSysMenuTypeBySql' parameterType='java.lang.String'>    ${value}  </delete>  <update id='updateSysMenuTypeBySql' parameterType='java.lang.String'>    ${value}  </update>  <select id='selectSysMenuTypeByCondition' parameterType='java.lang.String' resultMap='SysMenuTypeResultMap'>    select * from sys_menu_type where ${value}  </select>  <select id='selectCountByCondition' parameterType='java.lang.String' resultType='java.lang.Integer'>    select count(*) from sys_menu_type where ${value}  </select>  <select id='selectIntBySql' parameterType='java.lang.String' resultType='java.lang.Integer'>    ${value}  </select>  <select id='selectStringBySql' parameterType='java.lang.String' resultType='java.lang.String'>    ${value}  </select></mapper>